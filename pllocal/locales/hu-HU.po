#: pllocal/cog.py:52
#, docstring
msgid "Play local files and folders from the owner configured location"
msgstr "Helyi fájlok és mappák lejátszása a tulajdonos által konfigurált helyről"

#: pllocal/cog.py:158
#, docstring
msgid "Configure cog settings"
msgstr "Fogaskerék-beállítások konfigurálása"

#: pllocal/cog.py:162
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "A Cog és a PyLav verziójának megjelenítése"

#: pllocal/cog.py:178
msgid "Library / Cog"
msgstr "Könyvtár / Fogaskerekű"

#: pllocal/cog.py:179
msgid "Version"
msgstr "Verzió"

#: pllocal/cog.py:193
#, docstring
msgid "Update the track list for /local"
msgstr "A /local számlista frissítése"

#: pllocal/cog.py:203
msgid "I have updated my local track cache. There are now {number_variable_do_not_translate} tracks present."
msgstr "Frissítettem a helyi nyomkövetési gyorsítótárat. Most már vannak {number_variable_do_not_translate} pályák jelen vannak."

#: pllocal/cog.py:230
#, docstring
msgid "Play a local file or folder, supports partial searching"
msgstr "Helyi fájl vagy mappa lejátszása, támogatja a részleges keresést"

#: pllocal/cog.py:238
msgid "{user_input_query_variable_do_not_translate} is not a valid local file or folder."
msgstr "{user_input_query_variable_do_not_translate} nem érvényes helyi fájl vagy mappa."

#: pllocal/cog.py:256
msgid "You must be in a voice channel, so I can connect to it."
msgstr "Hangcsatornában kell lennie, hogy csatlakozhassak hozzá."

#: pllocal/cog.py:269
msgid "I do not have permission to connect or speak in {channel_variable_do_not_translate}."
msgstr "Nincs engedélyem arra, hogy csatlakozzak vagy beszéljek {channel_variable_do_not_translate}."

#: pllocal/cog.py:293
msgid "No tracks were found for your query."
msgstr "A rendszer nem talált nyomokat a lekérdezéshez."

#: pllocal/cog.py:295
msgid "{track_name_variable_do_not_translate} enqueued."
msgstr "{track_name_variable_do_not_translate} sorba állítva."

#: pllocal/cog.py:299
msgid "I have enqueued {track_count_variable_do_not_translate} tracks."
msgstr "Sorba álltam {track_count_variable_do_not_translate} sín."

