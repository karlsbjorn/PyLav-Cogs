#: pllocal/cog.py:52
#, docstring
msgid "Play local files and folders from the owner configured location"
msgstr "Přehrát místní soubory a složky od vlastníka nakonfigurovaného umístění"

#: pllocal/cog.py:158
#, docstring
msgid "Configure cog settings"
msgstr "Konfigurace nastavení cog"

#: pllocal/cog.py:162
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Zobrazit verzi Cog a PyLav"

#: pllocal/cog.py:178
msgid "Library / Cog"
msgstr "Knihovna / Cog"

#: pllocal/cog.py:179
msgid "Version"
msgstr "Verze"

#: pllocal/cog.py:193
#, docstring
msgid "Update the track list for /local"
msgstr "Aktualizovat seznam skladeb pro /local"

#: pllocal/cog.py:203
msgid "I have updated my local track cache. There are now {number_variable_do_not_translate} tracks present."
msgstr "Aktualizoval jsem místní mezipaměť tratí. Nyní existují {number_variable_do_not_translate} skladby přítomny."

#: pllocal/cog.py:230
#, docstring
msgid "Play a local file or folder, supports partial searching"
msgstr "Přehrát místní soubor nebo složku, podporuje částečné vyhledávání"

#: pllocal/cog.py:238
msgid "{user_input_query_variable_do_not_translate} is not a valid local file or folder."
msgstr "{user_input_query_variable_do_not_translate} není platný místní soubor nebo složka."

#: pllocal/cog.py:256
msgid "You must be in a voice channel, so I can connect to it."
msgstr "Musíte být v hlasovém kanálu, abych se k němu mohl připojit."

#: pllocal/cog.py:269
msgid "I do not have permission to connect or speak in {channel_variable_do_not_translate}."
msgstr "Nemám oprávnění k připojení nebo mluvení {channel_variable_do_not_translate}."

#: pllocal/cog.py:293
msgid "No tracks were found for your query."
msgstr "Pro váš dotaz nebyly nalezeny žádné stopy."

#: pllocal/cog.py:295
msgid "{track_name_variable_do_not_translate} enqueued."
msgstr "{track_name_variable_do_not_translate} Enqueued."

#: pllocal/cog.py:299
msgid "I have enqueued {track_count_variable_do_not_translate} tracks."
msgstr "Mám zařazené do fronty {track_count_variable_do_not_translate} Skladby."

