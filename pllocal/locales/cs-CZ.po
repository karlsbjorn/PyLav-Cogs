#: pllocal/cog.py:48
#, docstring
msgid "Play local files and folders from the owner configured location"
msgstr "Přehrát místní soubory a složky od vlastníka nakonfigurovaného umístění"

#: pllocal/cog.py:113
#, docstring
msgid "Configure cog settings"
msgstr "Konfigurace nastavení cog"

#: pllocal/cog.py:117
#, docstring
msgid "Show the version of the Cog and its PyLav dependencies"
msgstr "Zobrazení verze ozubeného kola a jeho závislostí PyLav"

#: pllocal/cog.py:133
msgid "Library/Cog"
msgstr "Knihovna/ozubené kolo"

#: pllocal/cog.py:134
msgid "Version"
msgstr "Verze"

#: pllocal/cog.py:148
#, docstring
msgid "Update the track list for /local"
msgstr "Aktualizovat seznam skladeb pro /local"

#: pllocal/cog.py:158
msgid "Local track list updated {number} currently present"
msgstr "Místní seznam skladeb aktualizoval {number} aktuálně přítomný"

