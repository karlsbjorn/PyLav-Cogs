#: pllocal/cog.py:48
#, docstring
msgid "Play local files and folders from the owner configured location"
msgstr "Възпроизвеждане на локални файлове и папки от конфигурираното от собственика местоположение"

#: pllocal/cog.py:113
#, docstring
msgid "Configure cog settings"
msgstr "Конфигуриране на настройките за зъбно колело"

#: pllocal/cog.py:117
#, docstring
msgid "Show the version of the Cog and its PyLav dependencies"
msgstr "Показване на версията на зъбното колело и неговите зависимости от PyLav"

#: pllocal/cog.py:133
msgid "Library/Cog"
msgstr "Библиотека/Зъбно колело"

#: pllocal/cog.py:134
msgid "Version"
msgstr "Версия"

#: pllocal/cog.py:148
#, docstring
msgid "Update the track list for /local"
msgstr "Актуализиране на списъка с песни за /local"

#: pllocal/cog.py:158
msgid "Local track list updated {number} currently present"
msgstr "Актуализиран е локалният списък с песни {number} В момента присъства"

#: pllocal/cog.py:183
#, docstring
msgid "Play a local file or folder, supports partial searching"
msgstr "Възпроизвеждане на локален файл или папка, поддържа частично търсене"

#: pllocal/cog.py:191
msgid "{query} is not a valid local file or folder"
msgstr "{query} не е валиден локален файл или папка"

#: pllocal/cog.py:207
msgid "You must be in a voice channel to allow me to connect"
msgstr "Трябва да сте в гласов канал, за да ми позволите да се свържа"

#: pllocal/cog.py:220
msgid "I don't have permission to connect or speak in {channel}"
msgstr "Нямам разрешение да се свързвам или да говоря в {channel}"

#: pllocal/cog.py:245
msgid "{track_count} tracks enqueued"
msgstr "{track_count} песни на опашка"

#: pllocal/cog.py:253
msgid "{track} enqueued"
msgstr "{track} На опашка"

#: pllocal/cog.py:263
msgid "No tracks were found for your query"
msgstr "Не са намерени следи за вашата заявка"

