#: pllocal/cog.py:52
#, docstring
msgid "Play local files and folders from the owner configured location"
msgstr "Възпроизвеждане на локални файлове и папки от конфигурираното от собственика местоположение"

#: pllocal/cog.py:158
#, docstring
msgid "Configure cog settings"
msgstr "Конфигуриране на настройките за зъбно колело"

#: pllocal/cog.py:162
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Показване на версията на Cog и PyLav"

#: pllocal/cog.py:178
msgid "Library / Cog"
msgstr "Библиотека / Зъбно колело"

#: pllocal/cog.py:179
msgid "Version"
msgstr "Версия"

#: pllocal/cog.py:193
#, docstring
msgid "Update the track list for /local"
msgstr "Актуализиране на списъка с песни за /local"

#: pllocal/cog.py:203
msgid "I have updated my local track cache. There are now {number_variable_do_not_translate} tracks present."
msgstr "Актуализирах локалния си кеш за песни. Сега има {number_variable_do_not_translate} налични следи."

#: pllocal/cog.py:230
#, docstring
msgid "Play a local file or folder, supports partial searching"
msgstr "Възпроизвеждане на локален файл или папка, поддържа частично търсене"

#: pllocal/cog.py:238
msgid "{user_input_query_variable_do_not_translate} is not a valid local file or folder."
msgstr "{user_input_query_variable_do_not_translate} не е валиден локален файл или папка."

#: pllocal/cog.py:256
msgid "You must be in a voice channel, so I can connect to it."
msgstr "Трябва да си в гласов канал, за да мога да се свържа с него."

#: pllocal/cog.py:269
msgid "I do not have permission to connect or speak in {channel_variable_do_not_translate}."
msgstr "Нямам разрешение да се свързвам или да говоря в {channel_variable_do_not_translate}."

#: pllocal/cog.py:293
msgid "No tracks were found for your query."
msgstr "Не са намерени следи за вашата заявка."

#: pllocal/cog.py:295
msgid "{track_name_variable_do_not_translate} enqueued."
msgstr "{track_name_variable_do_not_translate} на опашката."

#: pllocal/cog.py:299
msgid "I have enqueued {track_count_variable_do_not_translate} tracks."
msgstr "Аз съм се наредил на опашка {track_count_variable_do_not_translate} Песни."

