#: pllocal/cog.py:45
#, docstring
msgid "Play local files and folders from the owner configured location"
msgstr "Αναπαραγωγή τοπικών αρχείων και φακέλων από τη ρυθμισμένη τοποθεσία του ιδιοκτήτη"

#: pllocal/cog.py:58
#, docstring
msgid "Configure cog settings"
msgstr "Ρύθμιση ρυθμίσεων cog"

#: pllocal/cog.py:62
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Δείξτε την έκδοση του Γραναζιού και του PyLav"

#: pllocal/cog.py:78
msgid "Library / Cog"
msgstr "Βιβλιοθήκη / Γρανάζι"

#: pllocal/cog.py:79
msgid "Version"
msgstr "Εκδοχή"

#: pllocal/cog.py:93
#, docstring
msgid "Update the track list for /local"
msgstr "Ενημέρωση της λίστας κομματιών για /local"

#: pllocal/cog.py:103
msgid "I have updated my local track cache. There are now {number_variable_do_not_translate} tracks present."
msgstr "Έχω ενημερώσει την τοπική προσωρινή μνήμη κομματιών. Υπάρχουν τώρα {number_variable_do_not_translate} κομμάτια που υπάρχουν."

#: pllocal/cog.py:130
#, docstring
msgid "Play a local file or folder, supports partial searching"
msgstr "Αναπαραγωγή τοπικού αρχείου ή φακέλου, υποστηρίζει μερική αναζήτηση"

#: pllocal/cog.py:138
msgid "{user_input_query_variable_do_not_translate} is not a valid local file or folder."
msgstr "{user_input_query_variable_do_not_translate} δεν είναι έγκυρο τοπικό αρχείο ή φάκελος."

#: pllocal/cog.py:156
msgid "You must be in a voice channel, so I can connect to it."
msgstr "Πρέπει να βρίσκεστε σε ένα κανάλι φωνής, ώστε να μπορώ να συνδεθώ σε αυτό."

#: pllocal/cog.py:169
msgid "I do not have permission to connect or speak in {channel_variable_do_not_translate}."
msgstr "Δεν έχω δικαίωμα σύνδεσης ή ομιλίας {channel_variable_do_not_translate}."

#: pllocal/cog.py:195
msgid "No tracks were found for your query."
msgstr "Δεν βρέθηκαν κομμάτια για το ερώτημά σας."

#: pllocal/cog.py:197
msgid "{track_name_variable_do_not_translate} enqueued."
msgstr "{track_name_variable_do_not_translate} στην ουρά."

#: pllocal/cog.py:201
msgid "I have enqueued {track_count_variable_do_not_translate} tracks."
msgstr "Έχω εντάξει στην ουρά {track_count_variable_do_not_translate} Κομμάτια."

