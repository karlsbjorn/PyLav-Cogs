#: pllocal/cog.py:52
#, docstring
msgid "Play local files and folders from the owner configured location"
msgstr "Phát các tệp và thư mục cục bộ từ vị trí đã định cấu hình của chủ sở hữu"

#: pllocal/cog.py:158
#, docstring
msgid "Configure cog settings"
msgstr "Định cấu hình cài đặt bánh răng"

#: pllocal/cog.py:162
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Hiển thị phiên bản của Cog và PyLav"

#: pllocal/cog.py:178
msgid "Library / Cog"
msgstr "Thư viện / Cog"

#: pllocal/cog.py:179
msgid "Version"
msgstr "Phiên bản"

#: pllocal/cog.py:193
#, docstring
msgid "Update the track list for /local"
msgstr "Cập nhật danh sách theo dõi cho /local"

#: pllocal/cog.py:203
msgid "I have updated my local track cache. There are now {number_variable_do_not_translate} tracks present."
msgstr "Tôi đã cập nhật bộ nhớ cache theo dõi cục bộ của mình. Bây giờ đã có {number_variable_do_not_translate} theo dõi hiện tại."

#: pllocal/cog.py:230
#, docstring
msgid "Play a local file or folder, supports partial searching"
msgstr "Phát tệp hoặc thư mục cục bộ, hỗ trợ tìm kiếm một phần"

#: pllocal/cog.py:238
msgid "{user_input_query_variable_do_not_translate} is not a valid local file or folder."
msgstr "{user_input_query_variable_do_not_translate} không phải là tệp hoặc thư mục cục bộ hợp lệ."

#: pllocal/cog.py:256
msgid "You must be in a voice channel, so I can connect to it."
msgstr "Bạn phải ở trong một kênh thoại, vì vậy tôi có thể kết nối với nó."

#: pllocal/cog.py:269
msgid "I do not have permission to connect or speak in {channel_variable_do_not_translate}."
msgstr "Tôi không có quyền kết nối hoặc nói chuyện {channel_variable_do_not_translate}."

#: pllocal/cog.py:293
msgid "No tracks were found for your query."
msgstr "Không có bản nhạc nào được tìm thấy cho truy vấn của bạn."

#: pllocal/cog.py:295
msgid "{track_name_variable_do_not_translate} enqueued."
msgstr "{track_name_variable_do_not_translate} enqueued."

#: pllocal/cog.py:299
msgid "I have enqueued {track_count_variable_do_not_translate} tracks."
msgstr "Tôi đã đăng ký {track_count_variable_do_not_translate} Bài hát."

