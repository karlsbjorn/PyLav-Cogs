#: pllocal/cog.py:45
#, docstring
msgid "Play local files and folders from the owner configured location"
msgstr "Reproduciraj lokalne datoteke i mape s mjesta konfiguriranog od strane vlasnika"

#: pllocal/cog.py:58
#, docstring
msgid "Configure cog settings"
msgstr "Konfiguriranje postavki zupčanika"

#: pllocal/cog.py:62
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Prikaz verzije Cog i PyLav"

#: pllocal/cog.py:78
msgid "Library / Cog"
msgstr "Knjižnica / Zupčanik"

#: pllocal/cog.py:79
msgid "Version"
msgstr "Verzija"

#: pllocal/cog.py:93
#, docstring
msgid "Update the track list for /local"
msgstr "Ažuriranje popisa zapisa za /local"

#: pllocal/cog.py:103
msgid "I have updated my local track cache. There are now {number_variable_do_not_translate} tracks present."
msgstr "Ažurirao sam lokalnu predmemoriju zapisa. Sada ih ima. {number_variable_do_not_translate} prisutne pjesme."

#: pllocal/cog.py:130
#, docstring
msgid "Play a local file or folder, supports partial searching"
msgstr "Reproduciraj lokalnu datoteku ili mapu, podržava djelomično pretraživanje"

#: pllocal/cog.py:138
msgid "{user_input_query_variable_do_not_translate} is not a valid local file or folder."
msgstr "{user_input_query_variable_do_not_translate} nije valjana lokalna datoteka ili mapa."

#: pllocal/cog.py:156
msgid "You must be in a voice channel, so I can connect to it."
msgstr "Morate biti u glasovnom kanalu, tako da se mogu povezati s njim."

#: pllocal/cog.py:169
msgid "I do not have permission to connect or speak in {channel_variable_do_not_translate}."
msgstr "Nemam dozvolu za povezivanje ili govor u programu {channel_variable_do_not_translate}."

#: pllocal/cog.py:195
msgid "No tracks were found for your query."
msgstr "Za vaš upit nisu pronađene pjesme."

#: pllocal/cog.py:197
msgid "{track_name_variable_do_not_translate} enqueued."
msgstr "{track_name_variable_do_not_translate} stavljen u red čekanja."

#: pllocal/cog.py:201
msgid "I have enqueued {track_count_variable_do_not_translate} tracks."
msgstr "Ja sam enqueued {track_count_variable_do_not_translate} Pjesme."

