#: pllocal/cog.py:48
#, docstring
msgid "Play local files and folders from the owner configured location"
msgstr "Yerel dosya ve klasörleri, sahibinin yapılandırdığı konumdan oynatma"

#: pllocal/cog.py:113
#, docstring
msgid "Configure cog settings"
msgstr "Dişli çark ayarlarını yapılandırma"

#: pllocal/cog.py:117
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Cog ve PyLav'ın versiyonunu göster"

#: pllocal/cog.py:133
msgid "Library / Cog"
msgstr "Kütüphane / Dişli"

#: pllocal/cog.py:134
msgid "Version"
msgstr "Sürüm"

#: pllocal/cog.py:148
#, docstring
msgid "Update the track list for /local"
msgstr "/local için parça listesini güncelleme"

#: pllocal/cog.py:158
msgid "I have updated my local track cache. There are now {number_variable_do_not_translate} tracks present."
msgstr "Yerel parça önbelleğimi güncelledim. Şimdi var {number_variable_do_not_translate} mevcut izler."

#: pllocal/cog.py:185
#, docstring
msgid "Play a local file or folder, supports partial searching"
msgstr "Yerel bir dosya veya klasörü oynatın, kısmi aramayı destekler"

#: pllocal/cog.py:193
msgid "{user_input_query_variable_do_not_translate} is not a valid local file or folder."
msgstr "{user_input_query_variable_do_not_translate} geçerli bir yerel dosya veya klasör değil."

#: pllocal/cog.py:211
msgid "You must be in a voice channel, so I can connect to it."
msgstr "Bir ses kanalında olmalısın, böylece ona bağlanabilirim."

#: pllocal/cog.py:224
msgid "I do not have permission to connect or speak in {channel_variable_do_not_translate}."
msgstr "Bağlanma veya konuşma iznim yok {channel_variable_do_not_translate}."

#: pllocal/cog.py:248
msgid "No tracks were found for your query."
msgstr "Sorgunuz için hiçbir parça bulunamadı."

#: pllocal/cog.py:250
msgid "{track_name_variable_do_not_translate} enqueued."
msgstr "{track_name_variable_do_not_translate} Enqueued."

#: pllocal/cog.py:254
msgid "I have enqueued {track_count_variable_do_not_translate} tracks."
msgstr "Sıraya aldım {track_count_variable_do_not_translate} Parça."

