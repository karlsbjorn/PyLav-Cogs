#: pllocal/cog.py:52
#, docstring
msgid "Play local files and folders from the owner configured location"
msgstr "Redă fişierele locale şi dosarele de la locaţia configurată de proprietar"

#: pllocal/cog.py:158
#, docstring
msgid "Configure cog settings"
msgstr "Configurare setări cog"

#: pllocal/cog.py:162
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Afișați versiunea Cog și PyLav"

#: pllocal/cog.py:178
msgid "Library / Cog"
msgstr "Biblioteca / Cog"

#: pllocal/cog.py:179
msgid "Version"
msgstr "Versiune"

#: pllocal/cog.py:193
#, docstring
msgid "Update the track list for /local"
msgstr "Actualizează lista pieselor pentru /local"

#: pllocal/cog.py:203
msgid "I have updated my local track cache. There are now {number_variable_do_not_translate} tracks present."
msgstr "Mi-am actualizat memoria cache locală a pistei. Există acum {number_variable_do_not_translate} piese prezente."

#: pllocal/cog.py:230
#, docstring
msgid "Play a local file or folder, supports partial searching"
msgstr "Redă un fișier sau un dosar local, suportă căutarea parțială"

#: pllocal/cog.py:238
msgid "{user_input_query_variable_do_not_translate} is not a valid local file or folder."
msgstr "{user_input_query_variable_do_not_translate} nu este un fișier sau folder local valid."

#: pllocal/cog.py:256
msgid "You must be in a voice channel, so I can connect to it."
msgstr "Trebuie să fie într-un canal de voce, așa că am putea conecta la ea."

#: pllocal/cog.py:269
msgid "I do not have permission to connect or speak in {channel_variable_do_not_translate}."
msgstr "Nu am permisiunea de a mă conecta sau de a vorbi în {channel_variable_do_not_translate}."

#: pllocal/cog.py:293
msgid "No tracks were found for your query."
msgstr "Nu s-au găsit piste pentru interogarea dvs."

#: pllocal/cog.py:295
msgid "{track_name_variable_do_not_translate} enqueued."
msgstr "{track_name_variable_do_not_translate} enqueued."

#: pllocal/cog.py:299
msgid "I have enqueued {track_count_variable_do_not_translate} tracks."
msgstr "Am enqueued {track_count_variable_do_not_translate} Piese."

