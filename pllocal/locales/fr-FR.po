#: pllocal/cog.py:52
#, docstring
msgid "Play local files and folders from the owner configured location"
msgstr "Jouer des fichiers et des dossiers locaux depuis l'emplacement configuré du propriétaire"

#: pllocal/cog.py:158
#, docstring
msgid "Configure cog settings"
msgstr "Configurer les paramètres de cog"

#: pllocal/cog.py:162
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Montrer la version du Cog et du PyLav"

#: pllocal/cog.py:178
msgid "Library / Cog"
msgstr "Bibliothèque / Engrenage"

#: pllocal/cog.py:179
msgid "Version"
msgstr "Version"

#: pllocal/cog.py:193
#, docstring
msgid "Update the track list for /local"
msgstr "Mettre à jour la liste des pistes pour /local"

#: pllocal/cog.py:203
msgid "I have updated my local track cache. There are now {number_variable_do_not_translate} tracks present."
msgstr "J’ai mis à jour mon cache de piste local. Il y a maintenant {number_variable_do_not_translate} pistes présentes."

#: pllocal/cog.py:230
#, docstring
msgid "Play a local file or folder, supports partial searching"
msgstr "Jouer un fichier ou un dossier local, supporte la recherche partielle"

#: pllocal/cog.py:238
msgid "{user_input_query_variable_do_not_translate} is not a valid local file or folder."
msgstr "{user_input_query_variable_do_not_translate} n’est pas un fichier ou un dossier local valide."

#: pllocal/cog.py:256
msgid "You must be in a voice channel, so I can connect to it."
msgstr "Vous devez être dans un canal vocal, pour que je puisse m’y connecter."

#: pllocal/cog.py:269
msgid "I do not have permission to connect or speak in {channel_variable_do_not_translate}."
msgstr "Je n’ai pas la permission de me connecter ou de parler {channel_variable_do_not_translate}."

#: pllocal/cog.py:293
msgid "No tracks were found for your query."
msgstr "Aucune trace n’a été trouvée pour votre requête."

#: pllocal/cog.py:295
msgid "{track_name_variable_do_not_translate} enqueued."
msgstr "{track_name_variable_do_not_translate} en file d’attente."

#: pllocal/cog.py:299
msgid "I have enqueued {track_count_variable_do_not_translate} tracks."
msgstr "J’ai mis en file d’attente {track_count_variable_do_not_translate} traces."

