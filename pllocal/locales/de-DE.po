#: pllocal/cog.py:52
#, docstring
msgid "Play local files and folders from the owner configured location"
msgstr "Lokale Dateien und Ordner vom Eigentümer konfiguriert abspielen"

#: pllocal/cog.py:158
#, docstring
msgid "Configure cog settings"
msgstr "Cog Einstellungen konfigurieren"

#: pllocal/cog.py:162
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Zeigen Sie die Version von Cog und PyLav"

#: pllocal/cog.py:178
msgid "Library / Cog"
msgstr "Bibliothek / Zahnrad"

#: pllocal/cog.py:179
msgid "Version"
msgstr "Version"

#: pllocal/cog.py:193
#, docstring
msgid "Update the track list for /local"
msgstr "Titelliste für /local aktualisieren"

#: pllocal/cog.py:203
msgid "I have updated my local track cache. There are now {number_variable_do_not_translate} tracks present."
msgstr "Ich habe meinen lokalen Track-Cache aktualisiert. Es gibt jetzt {number_variable_do_not_translate} Spuren vorhanden."

#: pllocal/cog.py:230
#, docstring
msgid "Play a local file or folder, supports partial searching"
msgstr "Abspielen einer lokalen Datei oder eines Ordners, unterstützt teilweise Suche"

#: pllocal/cog.py:238
msgid "{user_input_query_variable_do_not_translate} is not a valid local file or folder."
msgstr "{user_input_query_variable_do_not_translate} ist keine gültige lokale Datei oder kein gültiger lokaler Ordner."

#: pllocal/cog.py:256
msgid "You must be in a voice channel, so I can connect to it."
msgstr "Sie müssen sich in einem Sprachkanal befinden, damit ich eine Verbindung herstellen kann."

#: pllocal/cog.py:269
msgid "I do not have permission to connect or speak in {channel_variable_do_not_translate}."
msgstr "Ich habe keine Berechtigung, mich zu verbinden oder zu sprechen {channel_variable_do_not_translate}."

#: pllocal/cog.py:293
msgid "No tracks were found for your query."
msgstr "Für Ihre Abfrage wurden keine Spuren gefunden."

#: pllocal/cog.py:295
msgid "{track_name_variable_do_not_translate} enqueued."
msgstr "{track_name_variable_do_not_translate} in die Warteschlange gestellt."

#: pllocal/cog.py:299
msgid "I have enqueued {track_count_variable_do_not_translate} tracks."
msgstr "Ich habe mich in die Warteschlange gestellt {track_count_variable_do_not_translate} Fährten."

