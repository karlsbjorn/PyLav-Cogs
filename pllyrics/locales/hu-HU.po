#: pllyrics/cog.py:31 pllyrics/cog.py:45
#, docstring
msgid "Lyrics commands for PyLav"
msgstr "Dalszöveg parancsok a PyLav számára"

#: pllyrics/cog.py:49
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "A Cog és a PyLav verziójának megjelenítése"

#: pllyrics/cog.py:65
msgid "Library / Cog"
msgstr "Könyvtár / Fogaskerekű"

#: pllyrics/cog.py:66
msgid "Version"
msgstr "Verzió"

#: pllyrics/cog.py:81
#, docstring
msgid "Set the lyrics settings"
msgstr "A dalszöveg beállításainak megadása"

#: pllyrics/cog.py:85
#, docstring
msgid "Toggle timed lyrics"
msgstr "Időzített dalszövegek váltása"

#: pllyrics/cog.py:90
msgid "I have now turned off timed lyrics in this server."
msgstr "Most kikapcsoltam az időzített dalszövegeket ezen a szerveren."

#: pllyrics/cog.py:92
msgid "I have now turned on timed lyrics in this server. I will send the lyrics as the track progresses if I can find lyrics for the playing track."
msgstr "Most bekapcsoltam az időzített dalszövegeket ezen a szerveren. A dalszöveget a szám előrehaladtával küldöm el, ha találok dalszöveget a lejátszott számhoz."

#: pllyrics/cog.py:106
#, docstring
msgid "Show full lyrics on track start timed lyrics"
msgstr "Teljes dalszöveg megjelenítése a pálya kezdési időzített dalszövegein"

#: pllyrics/cog.py:112
msgid "I have now turned off full lyrics in this server."
msgstr "Most kikapcsoltam a teljes dalszöveget ezen a szerveren."

#: pllyrics/cog.py:114
msgid "I have now turned on full lyrics on this server. I will send a message with the full lyrics at the start of a song if I can find lyrics for the playing track."
msgstr "Most bekapcsoltam a teljes dalszöveget ezen a szerveren. A dal elején küldök egy üzenetet a teljes szöveggel, ha találok szöveget a lejátszott számhoz."

#: pllyrics/cog.py:130
#, docstring
msgid "Set the lyrics channel"
msgstr "A dalszövegcsatorna beállítása"

#: pllyrics/cog.py:135
msgid "I have set the channel I will use to send lyrics to {channel_variable_do_not_translate}."
msgstr "Beállítottam azt a csatornát, amelyre dalszövegeket fogok küldeni {channel_variable_do_not_translate}."

#: pllyrics/cog.py:146
msgid "From now on, I will not send lyrics to any channel."
msgstr "Mostantól nem küldök dalszövegeket egyetlen csatornára sem."

#: pllyrics/cog.py:155
#, docstring
msgid "Show lyrics for current track if available"
msgstr "Az aktuális szám dalszövegeinek megjelenítése, ha rendelkezésre állnak"

#: pllyrics/cog.py:164
msgid "I am not currently playing anything on this server."
msgstr "Jelenleg nem játszom semmit ezen a szerveren."

#: pllyrics/cog.py:206
msgid "I have encountered an error while looking for the lyrics for this song: {error_variable_do_not_translate}."
msgstr "Hibába ütköztem, miközben a dal szövegét kerestem: {error_variable_do_not_translate}."

#: pllyrics/cog.py:218
msgid "The Flowery API, which I use to find lyrics, has returned an error while looking for the lyrics for this song {error_variable_do_not_translate}."
msgstr "A Flowery API, amelyet a dalszövegek keresésére használok, hibát adott vissza, miközben a dal szövegét kereste {error_variable_do_not_translate}."

