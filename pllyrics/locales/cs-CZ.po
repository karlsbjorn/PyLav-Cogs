#: pllyrics/cog.py:31 pllyrics/cog.py:45
#, docstring
msgid "Lyrics commands for PyLav"
msgstr "Příkazy s texty pro PyLav"

#: pllyrics/cog.py:49
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Zobrazit verzi Cog a PyLav"

#: pllyrics/cog.py:65
msgid "Library / Cog"
msgstr "Knihovna / Cog"

#: pllyrics/cog.py:66
msgid "Version"
msgstr "Verze"

#: pllyrics/cog.py:81
#, docstring
msgid "Set the lyrics settings"
msgstr "Nastavit nastavení textů"

#: pllyrics/cog.py:85
#, docstring
msgid "Toggle timed lyrics"
msgstr "Přepnout časované texty"

#: pllyrics/cog.py:90
msgid "I have now turned off timed lyrics in this server."
msgstr "Nyní jsem vypnul časované texty na tomto serveru."

#: pllyrics/cog.py:92
msgid "I have now turned on timed lyrics in this server. I will send the lyrics as the track progresses if I can find lyrics for the playing track."
msgstr "Nyní jsem zapnul časované texty na tomto serveru. Pošlu text, jak bude skladba postupovat, pokud najdu text pro přehrávanou stopu."

#: pllyrics/cog.py:106
#, docstring
msgid "Show full lyrics on track start timed lyrics"
msgstr "Zobrazit úplný text na začátku trasy"

#: pllyrics/cog.py:112
msgid "I have now turned off full lyrics in this server."
msgstr "Nyní jsem vypnul plné texty na tomto serveru."

#: pllyrics/cog.py:114
msgid "I have now turned on full lyrics on this server. I will send a message with the full lyrics at the start of a song if I can find lyrics for the playing track."
msgstr "Nyní jsem na tomto serveru zapnul plné texty. Pošlu zprávu s plným textem na začátku skladby, pokud najdu text pro přehrávanou skladbu."

#: pllyrics/cog.py:130
#, docstring
msgid "Set the lyrics channel"
msgstr "Nastavit kanál textů"

#: pllyrics/cog.py:135
msgid "I have set the channel I will use to send lyrics to {channel_variable_do_not_translate}."
msgstr "Nastavil jsem kanál, na který budu texty posílat {channel_variable_do_not_translate}."

#: pllyrics/cog.py:146
msgid "From now on, I will not send lyrics to any channel."
msgstr "Od této chvíle nebudu posílat texty na žádný kanál."

#: pllyrics/cog.py:155
#, docstring
msgid "Show lyrics for current track if available"
msgstr "Zobrazit text pro aktuální skladbu, pokud je k dispozici"

#: pllyrics/cog.py:164
msgid "I am not currently playing anything on this server."
msgstr "V současné době nehraji nic na tomto serveru."

#: pllyrics/cog.py:203
msgid "I have encountered an error while looking for the lyrics for this song: {error_variable_do_not_translate}."
msgstr "Při hledání textu této písně jsem narazil na chybu: {error_variable_do_not_translate}."

#: pllyrics/cog.py:215
msgid "The Flowery API, which I use to find lyrics, has returned an error while looking for the lyrics for this song {error_variable_do_not_translate}."
msgstr "Flowery API, které používám k hledání textů, vrátilo chybu při hledání textu pro tuto píseň {error_variable_do_not_translate}."

#: pllyrics/cog.py:227
msgid "No lyrics found"
msgstr "Nenalezeny žádné texty"

#: pllyrics/cog.py:242
msgid "I have found the lyrics for {title_variable_do_not_translate} by {author_variable_do_not_translate} - Part {page_variable_do_not_translate}."
msgstr "Našel jsem texty pro {title_variable_do_not_translate} vedle {author_variable_do_not_translate} -Část {page_variable_do_not_translate}."

#: pllyrics/cog.py:246
msgid "I have found the lyrics for {title_variable_do_not_translate} - Part {page_variable_do_not_translate}."
msgstr "Našel jsem texty pro {title_variable_do_not_translate} -Část {page_variable_do_not_translate}."

#: pllyrics/cog.py:252
msgid "(Guess) I have found the lyrics for {title_variable_do_not_translate} by {author_variable_do_not_translate} - Part {page_variable_do_not_translate}."
msgstr "(Hádejte) Našel jsem texty pro {title_variable_do_not_translate} vedle {author_variable_do_not_translate} -Část {page_variable_do_not_translate}."

#: pllyrics/cog.py:256
msgid "(Guess)I have found the lyrics for {title_variable_do_not_translate} - Part {page_variable_do_not_translate}."
msgstr "(Hádejte) Našel jsem texty pro {title_variable_do_not_translate} -Část {page_variable_do_not_translate}."

#: pllyrics/cog.py:262
msgid "I have found the lyrics for {title_variable_do_not_translate} by {author_variable_do_not_translate}."
