msgstr "(Indovina) Ho trovato il testo di {title_variable_do_not_translate} presso {author_variable_do_not_translate} -Parte {page_variable_do_not_translate}."

#: pllyrics/cog.py:256
msgid "(Guess)I have found the lyrics for {title_variable_do_not_translate} - Part {page_variable_do_not_translate}."
