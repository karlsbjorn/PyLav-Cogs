#: pllyrics/cog.py:31 pllyrics/cog.py:45
#, docstring
msgid "Lyrics commands for PyLav"
msgstr "Lời bài hát ra lệnh cho PyLav"

#: pllyrics/cog.py:49
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Hiển thị phiên bản của Cog và PyLav"

#: pllyrics/cog.py:65
msgid "Library / Cog"
msgstr "Thư viện / Cog"

#: pllyrics/cog.py:66
msgid "Version"
msgstr "Phiên bản"

#: pllyrics/cog.py:81
#, docstring
msgid "Set the lyrics settings"
msgstr "Đặt cài đặt lời bài hát"

#: pllyrics/cog.py:85
#, docstring
msgid "Toggle timed lyrics"
msgstr "Chuyển đổi lời bài hát theo thời gian"

#: pllyrics/cog.py:90
msgid "I have now turned off timed lyrics in this server."
msgstr "Bây giờ tôi đã tắt lời bài hát hẹn giờ trong máy chủ này."

#: pllyrics/cog.py:92
msgid "I have now turned on timed lyrics in this server. I will send the lyrics as the track progresses if I can find lyrics for the playing track."
msgstr "Bây giờ tôi đã bật lời bài hát hẹn giờ trong máy chủ này. Tôi sẽ gửi lời bài hát khi bản nhạc tiến triển nếu tôi có thể tìm thấy lời bài hát cho bản nhạc đang phát."

#: pllyrics/cog.py:106
#, docstring
msgid "Show full lyrics on track start timed lyrics"
msgstr "Hiển thị lời bài hát đầy đủ trên bài hát bắt đầu theo thời gian lời bài hát"

#: pllyrics/cog.py:112
msgid "I have now turned off full lyrics in this server."
msgstr "Bây giờ tôi đã tắt toàn bộ lời bài hát trong máy chủ này."

#: pllyrics/cog.py:114
msgid "I have now turned on full lyrics on this server. I will send a message with the full lyrics at the start of a song if I can find lyrics for the playing track."
msgstr "Bây giờ tôi đã bật lời bài hát đầy đủ trên máy chủ này. Tôi sẽ gửi một tin nhắn với lời bài hát đầy đủ ở đầu bài hát nếu tôi có thể tìm thấy lời bài hát cho bản nhạc đang phát."

#: pllyrics/cog.py:130
#, docstring
msgid "Set the lyrics channel"
msgstr "Đặt kênh lời bài hát"

#: pllyrics/cog.py:135
msgid "I have set the channel I will use to send lyrics to {channel_variable_do_not_translate}."
msgstr "Tôi đã thiết lập kênh mà tôi sẽ sử dụng để gửi lời bài hát đến {channel_variable_do_not_translate}."

#: pllyrics/cog.py:146
msgid "From now on, I will not send lyrics to any channel."
msgstr "Kể từ bây giờ, tôi sẽ không gửi lời bài hát đến bất kỳ kênh nào."

#: pllyrics/cog.py:155
#, docstring
msgid "Show lyrics for current track if available"
msgstr "Hiển thị lời bài hát cho bản nhạc hiện tại nếu có"

#: pllyrics/cog.py:164
msgid "I am not currently playing anything on this server."
msgstr "Tôi hiện không chơi bất cứ thứ gì trên máy chủ này."

#: pllyrics/cog.py:203
msgid "I have encountered an error while looking for the lyrics for this song: {error_variable_do_not_translate}."
msgstr "Tôi đã gặp phải một lỗi trong khi tìm kiếm lời bài hát cho bài hát này: {error_variable_do_not_translate}."

#: pllyrics/cog.py:215
msgid "The Flowery API, which I use to find lyrics, has returned an error while looking for the lyrics for this song {error_variable_do_not_translate}."
msgstr "API Flowery, mà tôi sử dụng để tìm lời bài hát, đã trả về một lỗi trong khi tìm kiếm lời bài hát cho bài hát này {error_variable_do_not_translate}."

#: pllyrics/cog.py:227
msgid "No lyrics found"
msgstr "Không tìm thấy lời bài hát"

#: pllyrics/cog.py:242
msgid "I have found the lyrics for {title_variable_do_not_translate} by {author_variable_do_not_translate} - Part {page_variable_do_not_translate}."
