#: pllyrics/cog.py:31 pllyrics/cog.py:45
#, docstring
msgid "Lyrics commands for PyLav"
msgstr "Lyrics kommandoer til PyLav"

#: pllyrics/cog.py:49
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Vis versionen af Cog og PyLav"

#: pllyrics/cog.py:65
msgid "Library / Cog"
msgstr "Bibliotek / Cog"

#: pllyrics/cog.py:66
msgid "Version"
msgstr "Version"

#: pllyrics/cog.py:81
#, docstring
msgid "Set the lyrics settings"
msgstr "Indstil sangtekstindstillingerne"

#: pllyrics/cog.py:85
#, docstring
msgid "Toggle timed lyrics"
msgstr "Slå timet sangtekst til/fra"

#: pllyrics/cog.py:90
msgid "I have now turned off timed lyrics in this server."
msgstr "Jeg har nu slået timede tekster fra på denne server."

#: pllyrics/cog.py:92
msgid "I have now turned on timed lyrics in this server. I will send the lyrics as the track progresses if I can find lyrics for the playing track."
msgstr "Jeg har nu tændt for tidsindstillede tekster på denne server. Jeg sender teksterne, efterhånden som nummeret skrider frem, hvis jeg kan finde tekster til afspilningssporet."

#: pllyrics/cog.py:106
#, docstring
msgid "Show full lyrics on track start timed lyrics"
msgstr "Vis fuld sangtekst ved start af spor"

#: pllyrics/cog.py:112
msgid "I have now turned off full lyrics in this server."
msgstr "Jeg har nu slået fulde tekster fra på denne server."

#: pllyrics/cog.py:114
msgid "I have now turned on full lyrics on this server. I will send a message with the full lyrics at the start of a song if I can find lyrics for the playing track."
msgstr "Jeg har nu tændt for fulde tekster på denne server. Jeg sender en besked med den fulde tekst i starten af en sang, hvis jeg kan finde tekst til afspilningssporet."

#: pllyrics/cog.py:130
#, docstring
msgid "Set the lyrics channel"
msgstr "Indstil sangtekstkanalen"

#: pllyrics/cog.py:135
msgid "I have set the channel I will use to send lyrics to {channel_variable_do_not_translate}."
msgstr "Jeg har indstillet den kanal, jeg vil bruge til at sende tekster til {channel_variable_do_not_translate}."

#: pllyrics/cog.py:146
msgid "From now on, I will not send lyrics to any channel."
msgstr "Fra nu af vil jeg ikke sende tekster til nogen kanal."

#: pllyrics/cog.py:155
#, docstring
msgid "Show lyrics for current track if available"
msgstr "Vis sangtekster for nuværende spor hvis tilgængeligt"

#: pllyrics/cog.py:164
msgid "I am not currently playing anything on this server."
msgstr "Jeg spiller i øjeblikket ikke noget på denne server."

#: pllyrics/cog.py:203
msgid "I have encountered an error while looking for the lyrics for this song: {error_variable_do_not_translate}."
msgstr "Jeg er stødt på en fejl, mens jeg ledte efter teksten til denne sang: {error_variable_do_not_translate}."

#: pllyrics/cog.py:215
msgid "The Flowery API, which I use to find lyrics, has returned an error while looking for the lyrics for this song {error_variable_do_not_translate}."
msgstr "Flowery API, som jeg bruger til at finde tekster, har returneret en fejl, mens jeg ledte efter teksten til denne sang {error_variable_do_not_translate}."

#: pllyrics/cog.py:227
msgid "No lyrics found"
msgstr "Ingen sangtekster fundet"

#: pllyrics/cog.py:242
msgid "I have found the lyrics for {title_variable_do_not_translate} by {author_variable_do_not_translate} - Part {page_variable_do_not_translate}."
msgstr "Jeg har fundet teksterne til {title_variable_do_not_translate} ved {author_variable_do_not_translate} -Del {page_variable_do_not_translate}."

#: pllyrics/cog.py:246
msgid "I have found the lyrics for {title_variable_do_not_translate} - Part {page_variable_do_not_translate}."
