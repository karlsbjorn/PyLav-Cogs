msgstr ""

#: plcontroller/cog.py:144
msgid "From now on, I will accept user requests in the controller channel."
msgstr ""

#: plcontroller/cog.py:152
msgid "From now on, I will ignore user requests in the controller channel."
msgstr ""

#: plcontroller/cog.py:166
msgid "I am not connected to a voice channel."
msgstr "Nisam povezan s glasovnim kanalom."

#: plcontroller/cog.py:177
msgid "Volume limit reached, player volume set to {volume_variable_do_not_translate}%."
msgstr ""

#: plcontroller/cog.py:188
msgid "Minimum volume reached, player volume set to 0%."
msgstr ""

#: plcontroller/cog.py:196
msgid "Player volume set to {volume_variable_do_not_translate}%"
msgstr "Glasnoća playera postavljena na {volume_variable_do_not_translate}%"

#: plcontroller/cog.py:216 plcontroller/cog.py:251 plcontroller/cog.py:293
#: plcontroller/cog.py:322 plcontroller/cog.py:356 plcontroller/cog.py:390
#: plcontroller/cog.py:416 plcontroller/view.py:468
msgid "I am not currently playing anything on this server."
msgstr "Trenutno ne igram ništa na ovom poslužitelju."

#: plcontroller/cog.py:224
msgid "From now on, I will now repeat the entire queue."
msgstr "Od sada ću ponavljati cijeli red."

#: plcontroller/cog.py:227
msgid "From now on, I will no longer repeat any tracks."
msgstr "Od sada više neću ponavljati tragove."

#: plcontroller/cog.py:231
msgid "From now on, I will now repeat {track_name_variable_do_not_translate}."
msgstr "Od sada, sada ću ponavljati. {track_name_variable_do_not_translate}."

#: plcontroller/cog.py:237
msgid "From now on, I will now repeat the current track."
msgstr "Od sada ću ponavljati trenutnu pjesmu."

#: plcontroller/cog.py:259
msgid "The server queue is currently empty."
msgstr "Red čekanja poslužitelja trenutno je prazan."

#: plcontroller/cog.py:267
msgid "You are not allowed to shuffle the queue."
msgstr "Nije vam dopušteno miješanje reda čekanja."

#: plcontroller/cog.py:275
msgid "{queue_size_variable_do_not_translate} tracks shuffled."
msgstr "{queue_size_variable_do_not_translate} tragovi su izmiješani."

#: plcontroller/cog.py:301
msgid "I have skipped {track_name_variable_do_not_translate} as requested."
msgstr "Preskočio sam {track_name_variable_do_not_translate} kao što je zatraženo."

#: plcontroller/cog.py:328
msgid "The player already resumed, did you mean to run {command_name_variable_do_not_translate}."
msgstr "Igrač je već nastavio, jeste li mislili trčati {command_name_variable_do_not_translate}."

#: plcontroller/cog.py:342
msgid "I have now resumed the player as requested."
msgstr "Sada sam nastavio s igračem kako sam tražio."

#: plcontroller/cog.py:362
msgid "The player is already paused, did you mean to run {command_name_variable_do_not_translate}."
msgstr "Igrač je već pauziran, jeste li mislili trčati {command_name_variable_do_not_translate}."

#: plcontroller/cog.py:376
msgid "I have now paused the player as requested."
msgstr "Sada sam pauzirao igrača kako sam tražio."

#: plcontroller/cog.py:398
msgid "I have stopped the playback and cleared the queue as requested."
msgstr "Zaustavio sam reprodukciju i očistio red kako sam tražio."

#: plcontroller/cog.py:425
msgid "The history of tracks is currently empty."
msgstr "Povijest pjesama trenutno je prazna."

#: plcontroller/cog.py:433
msgid "Playing previous track: {track_name_variable_do_not_translate}."
msgstr "Reproduciranje prethodne pjesme: {track_name_variable_do_not_translate}."

#: plcontroller/view.py:150 plcontroller/view.py:175 plcontroller/view.py:209
msgid "I am not connected to any voice channel at the moment."
msgstr "Trenutno nisam povezan ni s jednim glasovnim kanalom."

#: plcontroller/view.py:444
msgid "You must be in a voice channel to allow me to connect."
msgstr "Morate biti u glasovnom kanalu da biste mi omogućili da se povežem."

#: plcontroller/view.py:452
msgid "I do not have permission to connect or speak in {channel_variable_do_not_translate}."
msgstr "Nemam dozvolu za povezivanje ili govor u programu {channel_variable_do_not_translate}."

