#: plnodes/cog.py:32
#, docstring
msgid "Manage the nodes used by PyLav"
msgstr "Správa uzlů používaných PyLav"

#: plnodes/cog.py:43
#, docstring
msgid "Configure PyLav Nodes"
msgstr "Konfigurace PyLav uzlů"

#: plnodes/cog.py:47
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Zobrazit verzi Cog a PyLav"

#: plnodes/cog.py:63
msgid "Library / Cog"
msgstr "Knihovna / Cog"

#: plnodes/cog.py:64
msgid "Version"
msgstr "Verze"

#: plnodes/cog.py:77
#, docstring
msgid "Add a node to PyLav"
msgstr "Přidat uzel do PyLav"

#: plnodes/cog.py:87
msgid "Let us add a node to PyLav!"
msgstr "Přidáme uzel do PyLav!"

#: plnodes/cog.py:88
msgid "(**1**){space_variable_do_not_translate} - Apply changes and add the node to PyLav.\n"
"(**2**){space_variable_do_not_translate} - Cancel any changes made and close the menu.\n"
"(**3**){space_variable_do_not_translate} - Toggle between search-only and search and playback modes.\n"
"(**4**){space_variable_do_not_translate} - Toggle between SSL on and off.\n"
"(**5**){space_variable_do_not_translate} - Add a name to the node.\n"
"(**6**){space_variable_do_not_translate} - Add the host address of the node.\n"
"(**7**){space_variable_do_not_translate} - Add the port the node uses.\n"
"(**8**){space_variable_do_not_translate} - Set the password for the node.\n"
"(**9**){space_variable_do_not_translate} - Set the connection timeout.\n"
"(**10**) - Select which sources to disable for this node (Multiple can be selected).\n"
"If you interact with a button multiple times, only the last interaction will take effect.\n\n\n"
msgstr "(**1**){space_variable_do_not_translate} - Aplikujte změny a přidejte uzel do PyLav.\n"
"(**2**){space_variable_do_not_translate} - Zrušte všechny provedené změny a zavřete nabídku.\n"
"(**3**){space_variable_do_not_translate} - Přepínat mezi režimy vyhledávání a vyhledáváním a přehráváním.\n"
"(**4**){space_variable_do_not_translate} - Přepínat mezi SSL zapnuto a vypnuto.\n"
"(**5**){space_variable_do_not_translate} - Přidejte název uzlu.\n"
"(**6**){space_variable_do_not_translate} - Přidejte adresu hostitele uzlu.\n"
"(**7**){space_variable_do_not_translate} - Přidejte port, který uzel používá.\n"
"(**8**){space_variable_do_not_translate} - Nastavte heslo pro uzel.\n"
"(**9**){space_variable_do_not_translate} - Nastavte časový limit připojení.\n"
"(**10**) – Vyberte, které zdroje chcete pro tento uzel zakázat (lze vybrat více zdrojů).\n"
"Pokud s tlačítkem interagujete vícekrát, projeví se pouze poslední interakce.\n\n\n"

#: plnodes/cog.py:134
msgid "I have added the {name_variable_do_not_translate} node with the following settings:\n"
"Host: {host_variable_do_not_translate}\n"
"Port: {port_variable_do_not_translate}\n"
"Password: {password_variable_do_not_translate}\n"
"Resume Timeout: {resume_timeout_variable_do_not_translate}\n"
"Search Only: {search_only_variable_do_not_translate}\n"
"SSL: {ssl_variable_do_not_translate}\n"
"Disabled Sources: {disabled_sources_variable_do_not_translate}\n"
msgstr "Přidal jsem {name_variable_do_not_translate} s následujícím nastavením:\n"
"Hostitel: {host_variable_do_not_translate}\n"
"Přístav: {port_variable_do_not_translate}\n"
"Heslo: {password_variable_do_not_translate}\n"
"Časový limit obnovení: {resume_timeout_variable_do_not_translate}\n"
"Pouze vyhledávání: {search_only_variable_do_not_translate}\n"
"SSL: {ssl_variable_do_not_translate}\n"
"Zakázané zdroje: {disabled_sources_variable_do_not_translate}\n"

#: plnodes/cog.py:165 plnodes/cog.py:172
msgid "I am unable to add this node"
msgstr "Tento uzel nelze přidat"

#: plnodes/cog.py:178
#, docstring
msgid "Remove a node from a PyLav instance"
msgstr "Odstranit uzel z instance PyLav"

#: plnodes/cog.py:190
msgid "{name_variable_do_not_translate} is managed by PyLav and cannot be removed."
msgstr "{name_variable_do_not_translate} je spravován PyLav a nelze jej odebrat."

#: plnodes/cog.py:207
msgid "I have removed the {name_variable_do_not_translate} node.\n\n"
"{data_variable_do_not_translate}"
msgstr "Odstranil jsem {name_variable_do_not_translate} uzel.\n\n"
"{data_variable_do_not_translate}"

#: plnodes/cog.py:220
msgid "I have removed the {name_variable_do_not_translate} node. A direct message was sent to you with the node details in case you wish to re-add it."
msgstr "Odstranil jsem {name_variable_do_not_translate} uzel. Byla vám zaslána přímá zpráva s podrobnostmi o uzlu pro případ, že byste jej chtěli znovu přidat."

#: plnodes/cog.py:230
#, docstring
msgid "Manage all nodes in a PyLav instance"
msgstr "Správa všech uzlů v instanci PyLav"

#: plnodes/cog.py:242
msgid "Let us manage some nodes!"
msgstr "Nechte nás spravovat některé uzly!"

#: plnodes/cog.py:243
msgid "(**1**){space_variable_do_not_translate} - Cancel any changes made and close the menu.\n"
"(**6**){space_variable_do_not_translate} - Show sources enabled for this node.\n"
"(**7**){space_variable_do_not_translate} - Apply changes and add the node to PyLav.\n"
"(**8**){space_variable_do_not_translate} - Toggle between search-only and search and playback modes.\n"
"(**9**){space_variable_do_not_translate} - Toggle between SSL on and off.\n"
"(**10**) - Add a name to the node.\n"
"(**11**) - Add the host address of the node.\n"
"(**12**) - Add the port the node uses.\n"
"(**13**) - Set the password for the node.\n"
"(**14**) - Set the connection timeout.\n"
"(**15**) - Remove this node.\n"
"(**16**) - Select which sources to disable for this node (Multiple can be selected).\n"
"If you interact with a button multiple times, only the last interaction will take effect.\n\n\n"
msgstr "(**1**){space_variable_do_not_translate} - Zrušte všechny provedené změny a zavřete nabídku.\n"
"(**6**){space_variable_do_not_translate} - Zobrazit zdroje povolené pro tento uzel.\n"
"(**7**){space_variable_do_not_translate} - Aplikujte změny a přidejte uzel do PyLav.\n"
"(**8**){space_variable_do_not_translate} - Přepínat mezi režimy vyhledávání a vyhledáváním a přehráváním.\n"
"(**9**){space_variable_do_not_translate} - Přepínat mezi SSL zapnuto a vypnuto.\n"
"(**10**) – Přidejte název uzlu.\n"
"(**11**) – Přidejte adresu hostitele uzlu.\n"
"(**12**) – Přidejte port, který uzel používá.\n"
"(**13**) – Nastavte heslo pro uzel.\n"
"(**14**) – Nastavte časový limit připojení.\n"
"(**15**) – Odeberte tento uzel.\n"
"(**16**) – Vyberte, které zdroje chcete pro tento uzel zakázat (lze vybrat více zdrojů).\n"
"Pokud s tlačítkem interagujete vícekrát, projeví se pouze poslední interakce.\n\n\n"

#: plnodes/cog.py:272
msgid "{name_variable_do_not_translate} is managed by PyLav, and I can not modify it."
msgstr "{name_variable_do_not_translate} je spravován PyLav a nemohu jej upravit."

#: plnodes/cog.py:284
msgid "I have removed the {name_variable_do_not_translate} node."
msgstr "Odstranil jsem {name_variable_do_not_translate} uzel."

#: plnodes/cog.py:322
msgid "I have changed the {name_variable_do_not_translate} node to the following settings:\n"
"Host: {host_variable_do_not_translate}\n"
"Port: {port_variable_do_not_translate}\n"
"Password: {password_variable_do_not_translate}\n"
"Resume Timeout: {resume_timeout_variable_do_not_translate}\n"
"Search Only: {search_only_variable_do_not_translate}\n"
"SSL: {ssl_variable_do_not_translate}\n"
"Disabled Sources: {disabled_sources_variable_do_not_translate}\n"
msgstr "Změnil jsem {name_variable_do_not_translate} na následující nastavení:\n"
"Hostitel: {host_variable_do_not_translate}\n"
"Přístav: {port_variable_do_not_translate}\n"
"Heslo: {password_variable_do_not_translate}\n"
"Časový limit obnovení: {resume_timeout_variable_do_not_translate}\n"
"Pouze vyhledávání: {search_only_variable_do_not_translate}\n"
"SSL: {ssl_variable_do_not_translate}\n"
"Zakázané zdroje: {disabled_sources_variable_do_not_translate}\n"

