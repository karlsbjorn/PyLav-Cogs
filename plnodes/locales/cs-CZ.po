#: plnodes/cog.py:33
#, docstring
msgid "Manage the nodes used by PyLav"
msgstr "Správa uzlů používaných PyLav"

#: plnodes/cog.py:44
#, docstring
msgid "Configure PyLav Nodes"
msgstr "Konfigurace PyLav uzlů"

#: plnodes/cog.py:48
#, docstring
msgid "Show the version of the Cog and its PyLav dependencies"
msgstr "Zobrazení verze ozubeného kola a jeho závislostí PyLav"

#: plnodes/cog.py:64
msgid "Library/Cog"
msgstr "Knihovna/ozubené kolo"

#: plnodes/cog.py:65
msgid "Version"
msgstr "Verze"

#: plnodes/cog.py:78
#, docstring
msgid "Add a node to PyLav"
msgstr "Přidat uzel do PyLav"

#: plnodes/cog.py:88
msgid "Let's add a node to PyLav!"
msgstr "Pojďme přidat do PyLav!"

#: plnodes/cog.py:89
msgid "(**1**){space} - Apply changes and add the node to PyLav.\n"
"(**2**){space} - Cancel any changes made and close the menu.\n"
"(**3**){space} - Toggle between search-only and search and playback modes.\n"
"(**4**){space} - Toggle between SSL on and off.\n"
"(**5**){space} - Add a name to the node.\n"
"(**6**){space} - Add the host address of the node.\n"
"(**7**){space} - Add the port the node uses.\n"
"(**8**){space} - Set the password for the node.\n"
"(**9**){space} - Set the connection timeout.\n"
"(**10**) - Select which sources to disable for this node (Multiple can be selected).\n"
"If you interact with a button multiple times, only the last interaction will take effect.\n\n\n"
msgstr "(**1**){space} - Použít změny a přidat uzel do PyLav.\n"
"(**2**){space} - Zrušte všechny provedené změny a zavřete menu.\n"
"(**3**){space} - Přepíná mezi režimy pouze vyhledávání a přehrávání a vyhledávání.\n"
"(**4**){space} - Přepíná mezi SSL na a vypnutým\n"
"(**5**){space} - Přidej jméno do uzlu.\n"
"(**6**){space} - Přidejte adresu hostitele uzlu.\n"
"(**7**){space} - Přidejte port pro použití uzlu.\n"
"(**8**){space} - Nastavte heslo uzlu.\n"
"(**9**){space} - Nastavte časový limit připojení.\n"
"(**10**) - Vyberte, které zdroje pro vypnutí tohoto uzlu (lze vybrat více zdrojů).\n"
"Pokud budete s tlačítkem komunikovat vícekrát, projeví se pouze poslední interakce\n\n\n"

#: plnodes/cog.py:137
msgid "Added node {name} with the following settings:\n"
"Host: {host}\n"
"Port: {port}\n"
"Password: {password}\n"
"Resume Timeout: {resume_timeout}\n"
"Search Only: {search_only}\n"
"SSL: {ssl}\n"
"Disabled Sources: {disabled_sources}\n"
msgstr "Přidán uzel {name} s následujícími nastaveními:\n"
"Hostitel: {host}\n"
"Port: {port}\n"
"Heslo: {password}\n"
"Časový limit: {resume_timeout}\n"
"Pouze Hledat: {search_only}\n"
"SSL: {ssl}\n"
"Zakázané zdroje: {disabled_sources}\n"

#: plnodes/cog.py:168 plnodes/cog.py:175
msgid "Unable to add this node"
msgstr "Nelze přidat tento uzel"

#: plnodes/cog.py:181
#, docstring
msgid "Remove a node from a PyLav instance"
msgstr "Odstranit uzel z instance PyLav"

#: plnodes/cog.py:193
msgid "{name} is managed by PyLav and cannot be removed"
msgstr "{name} je spravován PyLav a nemůže být odstraněn"

#: plnodes/cog.py:208
msgid "Removed node {name}.\n\n"
"{data}"
msgstr "Odstraněný uzel {name}.\n\n"
"{data}"

#: plnodes/cog.py:216
msgid "Removed node {name}, a DM was sent to you with the node details in case you wish to re-add it"
msgstr "Odstraněný uzel {name}byl vám zaslán DM s detaily uzlu, pokud jej chcete znovu přidat"

#: plnodes/cog.py:226
#, docstring
msgid "Manage all nodes in a PyLav instance"
msgstr "Správa všech uzlů v instanci PyLav"

#: plnodes/cog.py:238
msgid "Let's manage some nodes!"
msgstr "Pojďme spravovat nějaké uzly!"

#: plnodes/cog.py:239
msgid "(**1**){space} - Cancel any changes made and close the menu.\n"
"(**6**){space} - Show sources enabled for this node.\n"
"(**7**){space} - Apply changes and add the node to PyLav.\n"
"(**8**){space} - Toggle between search-only and search and playback modes.\n"
"(**9**){space} - Toggle between SSL on and off.\n"
"(**10**) - Add a name to the node.\n"
"(**11**) - Add the host address of the node.\n"
"(**12**) - Add the port the node uses.\n"
"(**13**) - Set the password for the node.\n"
"(**14**) - Set the connection timeout.\n"
"(**15**) - Remove this node.\n"
"(**16**) - Select which sources to disable for this node (Multiple can be selected).\n"
"If you interact with a button multiple times, only the last interaction will take effect.\n\n\n"
msgstr "(**1**){space} - Zrušte všechny provedené změny a zavřete menu.\n"
"(**6**){space} - Zobrazit zdroje povolené pro tento uzel.\n"
"(**7**){space} - Použít změny a přidat uzel do PyLav.\n"
"(**8**){space} - Přepíná mezi režimy pouze vyhledávání a vyhledávání a přehrávání.\n"
"(**9**){space} - Přepíná mezi zapnutím a vypnutím SSL.\n"
"(**10**) - Přidat jméno do uzlu.\n"
"(**11**) - Přidat adresu hostitele.\n"
"(**12**) - Přidejte port použití uzlu.\n"
"(**13**) - Nastavte heslo pro uzel.\n"
"(**14**) - Nastavte časový limit připojení.\n"
"(**15**) - Odstraňte tento uzel.\n"
"(**16**) - Vyberte, které zdroje mají být pro tento uzel zakázány.\n"
"Pokud budete s tlačítkem komunikovat vícekrát, projeví se pouze poslední interakce.\n\n\n"

#: plnodes/cog.py:268
msgid "{name} is managed by PyLav and cannot be modified with this command"
msgstr "{name} je spravován PyLav a nemůže být upraven tímto příkazem"

#: plnodes/cog.py:280
msgid "Removed node {name}"
msgstr "Odstraněný uzel {name}"

#: plnodes/cog.py:316
msgid "Changed node {name} to the following settings:\n"
"Host: {host}\n"
"Port: {port}\n"
"Password: {password}\n"
"Resume Timeout: {resume_timeout}\n"
"Search Only: {search_only}\n"
"SSL: {ssl}\n"
"Disabled Sources: {disabled_sources}\n"
msgstr "Změněn uzel {name} na následující nastavení:\n"
"Hostitel: {host}\n"
"Port: {port}\n"
"Heslo: {password}\n"
"Časový limit obnovení: {resume_timeout}\n"
"Pouze Hledat: {search_only}\n"
"SSL: {ssl}\n"
"Zakázané zdroje: {disabled_sources}\n"

#: plnodes/cog.py:343
#, docstring
msgid "List all nodes used by PyLav"
msgstr "Seznam všech uzlů používaných PyLav"

#: plnodes/cog.py:353
msgid "No nodes added to PyLav"
msgstr "Do PyLav nebyly přidány žádné uzly"

