#: plnodes/cog.py:32
#, docstring
msgid "Manage the nodes used by PyLav"
msgstr "Tvarkykite PyLav naudojamus mazgus"

#: plnodes/cog.py:43
#, docstring
msgid "Configure PyLav Nodes"
msgstr "Konfigūruokite PyLav mazgus"

#: plnodes/cog.py:47
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Rodyti krumpliaračio ir PyLav versiją"

#: plnodes/cog.py:63
msgid "Library / Cog"
msgstr "Biblioteka / krumpliaratis"

#: plnodes/cog.py:64
msgid "Version"
msgstr "Versija"

#: plnodes/cog.py:77
#, docstring
msgid "Add a node to PyLav"
msgstr "Pridėkite mazgą prie PyLav"

#: plnodes/cog.py:87
msgid "Let us add a node to PyLav!"
msgstr "Pridėkime mazgą prie PyLav!"

#: plnodes/cog.py:88
msgid "(**1**){space_variable_do_not_translate} - Apply changes and add the node to PyLav.\n"
"(**2**){space_variable_do_not_translate} - Cancel any changes made and close the menu.\n"
"(**3**){space_variable_do_not_translate} - Toggle between search-only and search and playback modes.\n"
"(**4**){space_variable_do_not_translate} - Toggle between SSL on and off.\n"
"(**5**){space_variable_do_not_translate} - Add a name to the node.\n"
"(**6**){space_variable_do_not_translate} - Add the host address of the node.\n"
"(**7**){space_variable_do_not_translate} - Add the port the node uses.\n"
"(**8**){space_variable_do_not_translate} - Set the password for the node.\n"
"(**9**){space_variable_do_not_translate} - Set the connection timeout.\n"
"(**10**) - Select which sources to disable for this node (Multiple can be selected).\n"
"If you interact with a button multiple times, only the last interaction will take effect.\n\n\n"
msgstr "(**1**){space_variable_do_not_translate} - Taikykite pakeitimus ir pridėkite mazgą prie PyLav.\n"
"(**2**){space_variable_do_not_translate} - Atšaukite visus atliktus pakeitimus ir uždarykite meniu.\n"
"(**3**){space_variable_do_not_translate} - Perjunkite tik paieškos ir paieškos bei atkūrimo režimus.\n"
"(**4**){space_variable_do_not_translate} - Perjungti SSL įjungimą ir išjungimą.\n"
"(**5**){space_variable_do_not_translate} - Pridėkite pavadinimą prie mazgo.\n"
"(**6**){space_variable_do_not_translate} - Pridėkite mazgo pagrindinio kompiuterio adresą.\n"
"(**7**){space_variable_do_not_translate} - Pridėkite mazgo naudojamą prievadą.\n"
"(**8**){space_variable_do_not_translate} - Nustatykite mazgo slaptažodį.\n"
"(**9**){space_variable_do_not_translate} - Nustatykite ryšio skirtąjį laiką.\n"
"(**10**) - Pasirinkite, kuriuos šaltinius išjungti šiam mazgui (galima pasirinkti kelis).\n"
"Jei kelis kartus sąveikausite su mygtuku, įsigalios tik paskutinė sąveika.\n\n\n"

#: plnodes/cog.py:134
msgid "I have added the {name_variable_do_not_translate} node with the following settings:\n"
"Host: {host_variable_do_not_translate}\n"
"Port: {port_variable_do_not_translate}\n"
"Password: {password_variable_do_not_translate}\n"
"Resume Timeout: {resume_timeout_variable_do_not_translate}\n"
"Search Only: {search_only_variable_do_not_translate}\n"
"SSL: {ssl_variable_do_not_translate}\n"
"Disabled Sources: {disabled_sources_variable_do_not_translate}\n"
msgstr "Aš pridėjau {name_variable_do_not_translate} mazgas su šiais nustatymais:\n"
"Priimančiosios: {host_variable_do_not_translate}\n"
"Uostas: {port_variable_do_not_translate}\n"
"Slaptažodis: {password_variable_do_not_translate}\n"
"Tęsti skirtąjį laiką: {resume_timeout_variable_do_not_translate}\n"
"Tik ieškokite: {search_only_variable_do_not_translate}\n"
"SSL: {ssl_variable_do_not_translate}\n"
"Neįgalūs šaltiniai: {disabled_sources_variable_do_not_translate}\n"

#: plnodes/cog.py:165 plnodes/cog.py:172
msgid "I am unable to add this node"
msgstr "Negaliu pridėti šio mazgo"

#: plnodes/cog.py:178
#, docstring
msgid "Remove a node from a PyLav instance"
msgstr "Pašalinkite mazgą iš PyLav egzemplioriaus"

#: plnodes/cog.py:190
msgid "{name_variable_do_not_translate} is managed by PyLav and cannot be removed."
msgstr "{name_variable_do_not_translate} valdo PyLav ir negali būti pašalintas."

#: plnodes/cog.py:207
msgid "I have removed the {name_variable_do_not_translate} node.\n\n"
"{data_variable_do_not_translate}"
msgstr "Aš pašalinau {name_variable_do_not_translate} Mazgas.\n\n"
"{data_variable_do_not_translate}"

#: plnodes/cog.py:220
msgid "I have removed the {name_variable_do_not_translate} node. A direct message was sent to you with the node details in case you wish to re-add it."
msgstr "Aš pašalinau {name_variable_do_not_translate} Mazgas. Jums buvo išsiųstas tiesioginis pranešimas su išsamia mazgo informacija, jei norite jį iš naujo pridėti."

#: plnodes/cog.py:230
#, docstring
msgid "Manage all nodes in a PyLav instance"
msgstr "Valdykite visus \"PyLav\" egzemplioriaus mazgus"

#: plnodes/cog.py:242
msgid "Let us manage some nodes!"
msgstr "Valdykime kai kuriuos mazgus!"

#: plnodes/cog.py:243
msgid "(**1**){space_variable_do_not_translate} - Cancel any changes made and close the menu.\n"
"(**6**){space_variable_do_not_translate} - Show sources enabled for this node.\n"
"(**7**){space_variable_do_not_translate} - Apply changes and add the node to PyLav.\n"
"(**8**){space_variable_do_not_translate} - Toggle between search-only and search and playback modes.\n"
"(**9**){space_variable_do_not_translate} - Toggle between SSL on and off.\n"
"(**10**) - Add a name to the node.\n"
"(**11**) - Add the host address of the node.\n"
"(**12**) - Add the port the node uses.\n"
"(**13**) - Set the password for the node.\n"
"(**14**) - Set the connection timeout.\n"
"(**15**) - Remove this node.\n"
"(**16**) - Select which sources to disable for this node (Multiple can be selected).\n"
"If you interact with a button multiple times, only the last interaction will take effect.\n\n\n"
msgstr "(**1**){space_variable_do_not_translate} - Atšaukite visus atliktus pakeitimus ir uždarykite meniu.\n"
"(**6**){space_variable_do_not_translate} - Rodyti šiam mazgui įjungtus šaltinius.\n"
"(**7**){space_variable_do_not_translate} - Taikykite pakeitimus ir pridėkite mazgą prie PyLav.\n"
"(**8**){space_variable_do_not_translate} - Perjunkite tik paieškos ir paieškos bei atkūrimo režimus.\n"
"(**9**){space_variable_do_not_translate} - Perjungti SSL įjungimą ir išjungimą.\n"
"(**10**) - Pridėkite pavadinimą prie mazgo.\n"
"(**11**) - Pridėkite mazgo pagrindinio kompiuterio adresą.\n"
"(**12**) - Pridėkite mazgo naudojamą prievadą.\n"
"(**13**) - Nustatykite mazgo slaptažodį.\n"
"(**14**) - Nustatykite ryšio skirtąjį laiką.\n"
"(**15**) - Pašalinkite šį mazgą.\n"
"(**16**) - Pasirinkite, kuriuos šaltinius išjungti šiam mazgui (galima pasirinkti kelis).\n"
"Jei kelis kartus sąveikausite su mygtuku, įsigalios tik paskutinė sąveika.\n\n\n"

#: plnodes/cog.py:272
msgid "{name_variable_do_not_translate} is managed by PyLav, and I can not modify it."
msgstr "{name_variable_do_not_translate} valdo PyLav, ir aš negaliu jo modifikuoti."

#: plnodes/cog.py:284
msgid "I have removed the {name_variable_do_not_translate} node."
