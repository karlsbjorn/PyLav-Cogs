#: plnodes/cog.py:33
#, docstring
msgid "Manage the nodes used by PyLav"
msgstr "Upravljanje čvorovima koje koristi PyLav"

#: plnodes/cog.py:44
#, docstring
msgid "Configure PyLav Nodes"
msgstr "Konfiguriranje PyLav čvorova"

#: plnodes/cog.py:48
#, docstring
msgid "Show the version of the Cog and its PyLav dependencies"
msgstr "Prikaz verzije Zupčanika i njegovih PyLav ovisnosti"

#: plnodes/cog.py:64
msgid "Library/Cog"
msgstr "Biblioteka/Zupčanik"

#: plnodes/cog.py:65
msgid "Version"
msgstr "Verzija"

#: plnodes/cog.py:78
#, docstring
msgid "Add a node to PyLav"
msgstr "Dodavanje čvora u PyLav"

#: plnodes/cog.py:88
msgid "Let's add a node to PyLav!"
msgstr "Dodajmo čvor u PyLav!"

#: plnodes/cog.py:89
msgid "(**1**){space} - Apply changes and add the node to PyLav.\n"
"(**2**){space} - Cancel any changes made and close the menu.\n"
"(**3**){space} - Toggle between search-only and search and playback modes.\n"
"(**4**){space} - Toggle between SSL on and off.\n"
"(**5**){space} - Add a name to the node.\n"
"(**6**){space} - Add the host address of the node.\n"
"(**7**){space} - Add the port the node uses.\n"
"(**8**){space} - Set the password for the node.\n"
"(**9**){space} - Set the connection timeout.\n"
"(**10**) - Select which sources to disable for this node (Multiple can be selected).\n"
"If you interact with a button multiple times, only the last interaction will take effect.\n\n\n"
msgstr "(**1**){space} - Primijenite promjene i dodajte čvor u PyLav.\n"
"(**2**){space} - Otkažite sve izvršene promjene i zatvorite izbornik.\n"
"(**3**){space} - Prebacivanje između načina pretraživanja i pretraživanja i reprodukcije.\n"
"(**4**){space} - Prebacivanje između SSL-a uključeno i isključeno.\n"
"(**5**){space} - Dodajte naziv čvoru.\n"
"(**6**){space} - Dodajte adresu glavnog računala čvora.\n"
"(**7**){space} - Dodajte priključak koji čvor koristi.\n"
"(**8**){space} - Postavite lozinku za čvor.\n"
"(**9**){space} - Postavite vremensko ograničenje veze.\n"
"(**10**) - Odaberite koje izvore želite onemogućiti za ovaj čvor (može se odabrati višestruko).\n"
"Ako s gumbom stupite u interakciju više puta, stupit će na snagu samo posljednja interakcija.\n\n\n"

#: plnodes/cog.py:137
msgid "Added node {name} with the following settings:\n"
"Host: {host}\n"
"Port: {port}\n"
"Password: {password}\n"
"Resume Timeout: {resume_timeout}\n"
"Search Only: {search_only}\n"
"SSL: {ssl}\n"
"Disabled Sources: {disabled_sources}\n"
msgstr "Dodani čvor {name} sa sljedećim postavkama:\n"
"Domaćin: {host}\n"
"Luka: {port}\n"
"Lozinka: {password}\n"
"Prekoračenje vremena nastavka: {resume_timeout}\n"
"Samo pretraživanje: {search_only}\n"
"SSL: {ssl}\n"
"Onemogućeni izvori: {disabled_sources}\n"

#: plnodes/cog.py:168 plnodes/cog.py:175
msgid "Unable to add this node"
msgstr "Nije moguće dodati ovaj čvor"

#: plnodes/cog.py:181
#, docstring
msgid "Remove a node from a PyLav instance"
msgstr "Uklanjanje čvora iz instance PyLav-a"

#: plnodes/cog.py:193
msgid "{name} is managed by PyLav and cannot be removed"
msgstr "{name} njime upravlja PyLav i ne može se ukloniti"

#: plnodes/cog.py:208
msgid "Removed node {name}.\n\n"
"{data}"
msgstr "Uklonjeni čvor {name}.\n\n"
"{data}"

#: plnodes/cog.py:216
msgid "Removed node {name}, a DM was sent to you with the node details in case you wish to re-add it"
msgstr "Uklonjeni čvor {name}, poslan vam je DM s detaljima čvora u slučaju da ga želite ponovno dodati"

#: plnodes/cog.py:226
#, docstring
msgid "Manage all nodes in a PyLav instance"
msgstr "Upravljanje svim čvorovima u PyLav instanci"

#: plnodes/cog.py:238
msgid "Let's manage some nodes!"
msgstr "Upravljajmo nekim čvorovima!"

#: plnodes/cog.py:239
msgid "(**1**){space} - Cancel any changes made and close the menu.\n"
"(**6**){space} - Show sources enabled for this node.\n"
"(**7**){space} - Apply changes and add the node to PyLav.\n"
"(**8**){space} - Toggle between search-only and search and playback modes.\n"
"(**9**){space} - Toggle between SSL on and off.\n"
"(**10**) - Add a name to the node.\n"
"(**11**) - Add the host address of the node.\n"
"(**12**) - Add the port the node uses.\n"
"(**13**) - Set the password for the node.\n"
"(**14**) - Set the connection timeout.\n"
"(**15**) - Remove this node.\n"
"(**16**) - Select which sources to disable for this node (Multiple can be selected).\n"
"If you interact with a button multiple times, only the last interaction will take effect.\n\n\n"
msgstr "(**1**){space} - Otkažite sve izvršene promjene i zatvorite izbornik.\n"
"(**6**){space} - Prikaži izvore omogućene za ovaj čvor.\n"
"(**7**){space} - Primijenite promjene i dodajte čvor u PyLav.\n"
"(**8**){space} - Prebacivanje između načina pretraživanja i pretraživanja i reprodukcije.\n"
"(**9**){space} - Prebacivanje između SSL-a uključeno i isključeno.\n"
"(**10**) - Dodajte naziv čvoru.\n"
"(**11**) - Dodajte adresu glavnog računala čvora.\n"
"(**12**) - Dodajte priključak koji čvor koristi.\n"
"(**13**) - Postavite lozinku za čvor.\n"
"(**14**) - Postavite vremensko ograničenje veze.\n"
"(**15**) - Uklonite ovaj čvor.\n"
"(**16**) - Odaberite koje izvore želite onemogućiti za ovaj čvor (može se odabrati višestruko).\n"
"Ako s gumbom stupite u interakciju više puta, stupit će na snagu samo posljednja interakcija.\n\n\n"

#: plnodes/cog.py:268
msgid "{name} is managed by PyLav and cannot be modified with this command"
msgstr "{name} njime upravlja PyLav i ne može se mijenjati ovom naredbom"

#: plnodes/cog.py:280
msgid "Removed node {name}"
msgstr "Uklonjeni čvor {name}"

#: plnodes/cog.py:316
msgid "Changed node {name} to the following settings:\n"
"Host: {host}\n"
"Port: {port}\n"
"Password: {password}\n"
"Resume Timeout: {resume_timeout}\n"
"Search Only: {search_only}\n"
"SSL: {ssl}\n"
"Disabled Sources: {disabled_sources}\n"
msgstr "Promijenjen čvor {name} do sljedećih postavki:\n"
"Domaćin: {host}\n"
"Luka: {port}\n"
"Lozinka: {password}\n"
"Prekoračenje vremena nastavka: {resume_timeout}\n"
"Samo pretraživanje: {search_only}\n"
"SSL: {ssl}\n"
"Onemogućeni izvori: {disabled_sources}\n"

