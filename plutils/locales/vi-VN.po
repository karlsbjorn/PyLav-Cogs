#: plutils/cog.py:58 plutils/cog.py:68
#, docstring
msgid "Utility commands for PyLav"
msgstr "Các lệnh tiện ích cho PyLav"

#: plutils/cog.py:72
#, docstring
msgid "Show the version of the Cog and its PyLav dependencies"
msgstr "Hiển thị phiên bản của Cog và các phụ thuộc PyLav của nó"

#: plutils/cog.py:88
msgid "Library/Cog"
msgstr "Thư viện / Cog"

#: plutils/cog.py:89
msgid "Version"
msgstr "Phiên bản"

#: plutils/cog.py:102
#, docstring
msgid "Show the slashes available in the bot.\n\n"
"        Author: TrustyJAID#0001 via code block on Discord channel.\n"
"        "
msgstr "Hiển thị các dấu gạch chéo có sẵn trong bot.\n\n"
"        Tác giả: TrustyJAID # 0001 thông qua khối mã trên kênh Discord.\n"
"        "

#: plutils/cog.py:133
#, docstring
msgid "Get info about specific things"
msgstr "Nhận thông tin về những điều cụ thể"

#: plutils/cog.py:137
#, docstring
msgid "Get the base64 of the current track"
msgstr "Nhận base64 của bản nhạc hiện tại"

#: plutils/cog.py:145 plutils/cog.py:177 plutils/cog.py:209 plutils/cog.py:240
#: plutils/cog.py:273
msgid "Not connected to a voice channel"
msgstr "Không được kết nối với kênh thoại"

#: plutils/cog.py:153 plutils/cog.py:185 plutils/cog.py:217 plutils/cog.py:248
msgid "Nothing playing"
msgstr "Không có gì chơi"

#: plutils/cog.py:168
#, docstring
msgid "Get the author of the current track"
msgstr "Nhận tác giả của bản nhạc hiện tại"

#: plutils/cog.py:200
#, docstring
msgid "Get the title of the current track"
msgstr "Lấy tiêu đề của bản nhạc hiện tại"

#: plutils/cog.py:232
#, docstring
msgid "Get the source of the current track"
msgstr "Lấy nguồn của bản nhạc hiện tại"

#: plutils/cog.py:264
#, docstring
msgid "Get the API of the current track"
msgstr "Tải API của bản nhạc hiện tại"

#: plutils/cog.py:284 plutils/cog.py:293
msgid "Unable to get player info"
msgstr "Không thể lấy thông tin người chơi"

#: plutils/cog.py:309
#, docstring
msgid "Decode a track's base64 string into a JSON object"
msgstr "Giải mã chuỗi base64 của bản nhạc thành đối tượng JSON"

#: plutils/cog.py:318
msgid "Invalid base64 string"
msgstr "Chuỗi base64 không hợp lệ"

#: plutils/cog.py:334
#, docstring
msgid "Manage the query cache"
msgstr "Quản lý bộ đệm ẩn truy vấn"

#: plutils/cog.py:338
#, docstring
msgid "Clear the query cache"
msgstr "Xóa bộ đệm ẩn truy vấn"

#: plutils/cog.py:345 plutils/cog.py:374 plutils/cog.py:387
msgid "Query cache cleared"
msgstr "Đã xóa bộ nhớ cache truy vấn"

#: plutils/cog.py:351
#, docstring
msgid "Clear the query cache older than a number of days"
msgstr "Xóa bộ đệm ẩn truy vấn cũ hơn một số ngày"

#: plutils/cog.py:359
msgid "Days must be less than 31"
msgstr "Số ngày phải dưới 31"

#: plutils/cog.py:367
msgid "Days must be greater than 1"
msgstr "Số ngày phải lớn hơn 1"

#: plutils/cog.py:380
#, docstring
msgid "Clear the query cache for a query"
msgstr "Xóa bộ đệm ẩn truy vấn cho truy vấn"

#: plutils/cog.py:393
#, docstring
msgid "Get the size of the query cache"
msgstr "Lấy kích cỡ của bộ đệm ẩn truy vấn"

#: plutils/cog.py:400
msgid "Query cache size: `{size}`"
msgstr "Kích thước bộ nhớ cache truy vấn: '{size}`"

#: plutils/cog.py:411
#, docstring
msgid "Start memory tracing\n\n"
"        `[p]plutils trace 0` turns off tracing\n"
"        `[p]plutils trace 1` turns on tracing\n"
"        `[p]plutils trace` shows the current status of tracing\n"
"        "
msgstr "Bắt đầu theo dõi bộ nhớ\n\n"
"        `[p]plutils dấu vết 0 'tắt theo dõi\n"
"        `[p]plutils trace 1 'lượt trên truy tìm\n"
"        `[p]dấu vết plutils 'hiển thị tình trạng truy tìm hiện tại\n"
"        "

#: plutils/cog.py:425
msgid "Stopped memory tracing"
msgstr "Đã dừng theo dõi bộ nhớ"

#: plutils/cog.py:434
msgid "Started memory tracing"
msgstr "Bắt đầu theo dõi bộ nhớ"

#: plutils/cog.py:443
msgid "You need to start tracing first"
msgstr "Bạn cần bắt đầu theo dõi trước"

#: plutils/cog.py:455
#, docstring
msgid "Set the logger level\n\n"
"        Levels are the following:\n"
"        0: Critical\n"
"        1: Error\n"
"        2: Warning\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6: Trace\n"
"        "
msgstr "Đặt cấp độ logger\n\n"
"        Các cấp độ như sau:\n"
"        0: Quan trọng\n"
"        1: Lỗi\n"
"        2: Cảnh báo\n"
"        3: Thông tin\n"
"        4: Gỡ lỗi\n"
"        5: Verbose\n"
"        6: Theo dõi\n"
"        "

#: plutils/cog.py:486
msgid "Logger level set to `{level}`"
msgstr "Cấp độ logger được đặt thành '{level}`"

