#: plutils/cog.py:58 plutils/cog.py:68
#, docstring
msgid "Utility commands for PyLav"
msgstr "Các lệnh tiện ích cho PyLav"

#: plutils/cog.py:72
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Hiển thị phiên bản của Cog và PyLav"

#: plutils/cog.py:88
msgid "Library / Cog"
msgstr "Thư viện / Cog"

#: plutils/cog.py:89
msgid "Version"
msgstr "Phiên bản"

#: plutils/cog.py:102
#, docstring
msgid "Show the slashes available in the bot.\n\n"
"        Author: TrustyJAID#0001 via code block on Discord channel.\n"
"        "
msgstr "Hiển thị các dấu gạch chéo có sẵn trong bot.\n\n"
"        Tác giả: TrustyJAID # 0001 thông qua khối mã trên kênh Discord.\n"
"        "

#: plutils/cog.py:133
#, docstring
msgid "Get info about specific things"
msgstr "Nhận thông tin về những điều cụ thể"

#: plutils/cog.py:137
#, docstring
msgid "Get the base64 of the current track"
msgstr "Nhận base64 của bản nhạc hiện tại"

#: plutils/cog.py:145 plutils/cog.py:179 plutils/cog.py:213 plutils/cog.py:246
#: plutils/cog.py:281
msgid "You must be in a voice channel, so I can connect to it."
msgstr "Bạn phải ở trong một kênh thoại, vì vậy tôi có thể kết nối với nó."

#: plutils/cog.py:154 plutils/cog.py:188 plutils/cog.py:222 plutils/cog.py:255
msgid "I am not currently playing anything on this server."
msgstr "Tôi hiện không chơi bất cứ thứ gì trên máy chủ này."

#: plutils/cog.py:170
#, docstring
msgid "Get the author of the current track"
msgstr "Nhận tác giả của bản nhạc hiện tại"

#: plutils/cog.py:204
#, docstring
msgid "Get the title of the current track"
msgstr "Lấy tiêu đề của bản nhạc hiện tại"

#: plutils/cog.py:238
#, docstring
msgid "Get the source of the current track"
msgstr "Lấy nguồn của bản nhạc hiện tại"

#: plutils/cog.py:272
#, docstring
msgid "Get the API of the current track"
msgstr "Tải API của bản nhạc hiện tại"

#: plutils/cog.py:292 plutils/cog.py:301
msgid "Unable to get player info."
msgstr "Không thể lấy thông tin người chơi."

#: plutils/cog.py:317
#, docstring
msgid "Decode the track base64 string into a JSON object"
msgstr "Giải mã chuỗi base64 của bản nhạc thành một đối tượng JSON"

#: plutils/cog.py:326
msgid "Invalid base64 string."
msgstr "Chuỗi base64 không hợp lệ."

#: plutils/cog.py:342
#, docstring
msgid "Manage the query cache"
msgstr "Quản lý bộ đệm ẩn truy vấn"

#: plutils/cog.py:346
#, docstring
msgid "Clear the query cache"
msgstr "Xóa bộ đệm ẩn truy vấn"

#: plutils/cog.py:353 plutils/cog.py:382 plutils/cog.py:395
msgid "Query cache cleared."
msgstr "Truy vấn bộ nhớ cache bị xóa."

#: plutils/cog.py:359
#, docstring
msgid "Clear the query cache older than a number of days"
msgstr "Xóa bộ đệm ẩn truy vấn cũ hơn một số ngày"

#: plutils/cog.py:367
msgid "Days must be less than 31."
msgstr "Ngày phải dưới 31."

#: plutils/cog.py:375
msgid "Days must be greater than 1."
msgstr "Ngày phải lớn hơn 1."

#: plutils/cog.py:388
#, docstring
msgid "Clear the query cache for a query"
msgstr "Xóa bộ đệm ẩn truy vấn cho truy vấn"

#: plutils/cog.py:401
#, docstring
msgid "Get the size of the query cache"
msgstr "Lấy kích cỡ của bộ đệm ẩn truy vấn"

#: plutils/cog.py:408
msgid "Query cache size: `{size_variable_do_not_translate}`."
msgstr "Kích thước bộ nhớ cache truy vấn: '{size_variable_do_not_translate}`."

#: plutils/cog.py:419
#, docstring
msgid "Start memory tracing\n\n"
"        `[p]plutils trace 0` turns off tracing\n"
"        `[p]plutils trace 1` turns on tracing\n"
"        `[p]plutils trace` shows the current status of tracing\n"
"        "
msgstr "Bắt đầu theo dõi bộ nhớ\n\n"
"        `[p]plutils dấu vết 0 'tắt theo dõi\n"
"        `[p]plutils trace 1 'lượt trên truy tìm\n"
"        `[p]dấu vết plutils 'hiển thị tình trạng truy tìm hiện tại\n"
"        "

#: plutils/cog.py:433
msgid "I have stopped memory tracing"
msgstr "Tôi đã ngừng theo dõi bộ nhớ"

#: plutils/cog.py:442
msgid "I have started memory tracing."
msgstr "Tôi đã bắt đầu truy tìm trí nhớ."

#: plutils/cog.py:451
msgid "You need to start tracing first."
msgstr "Bạn cần bắt đầu theo dõi trước."

#: plutils/cog.py:463
#, docstring
msgid "Set the logger level\n\n"
"        Levels are the following:\n"
"        0: Critical\n"
"        1: Error\n"
"        2: Warning\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6: Trace\n"
"        "
msgstr "Đặt cấp độ logger\n\n"
"        Các cấp độ như sau:\n"
"        0: Quan trọng\n"
"        1: Lỗi\n"
"        2: Cảnh báo\n"
"        3: Thông tin\n"
"        4: Gỡ lỗi\n"
"        5: Verbose\n"
"        6: Theo dõi\n"
"        "

#: plutils/cog.py:494
msgid "Logger level set to `{level_variable_do_not_translate}`."
msgstr "Cấp độ logger được đặt thành '{level_variable_do_not_translate}`."

