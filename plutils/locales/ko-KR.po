#: plutils/cog.py:58 plutils/cog.py:68
#, docstring
msgid "Utility commands for PyLav"
msgstr "PyLav에 대한 유틸리티 명령"

#: plutils/cog.py:72
#, docstring
msgid "Show the version of the Cog and its PyLav dependencies"
msgstr "Cog 및 해당 PyLav 종속성의 버전 표시"

#: plutils/cog.py:88
msgid "Library/Cog"
msgstr "도서관/톱니바퀴"

#: plutils/cog.py:89
msgid "Version"
msgstr "버전"

#: plutils/cog.py:102
#, docstring
msgid "Show the slashes available in the bot.\n\n"
"        Author: TrustyJAID#0001 via code block on Discord channel.\n"
"        "
msgstr "봇에서 사용할 수 있는 슬래시를 표시합니다.\n\n"
"        저자: 디스코드 채널의 코드 블록을 통해 TrustyJAID#0001.\n"
"        "

#: plutils/cog.py:133
#, docstring
msgid "Get info about specific things"
msgstr "특정 항목에 대한 정보 얻기"

#: plutils/cog.py:137
#, docstring
msgid "Get the base64 of the current track"
msgstr "현재 트랙의 base64 가져오기"

#: plutils/cog.py:145 plutils/cog.py:177 plutils/cog.py:209 plutils/cog.py:240
#: plutils/cog.py:273
msgid "Not connected to a voice channel"
msgstr "음성 채널에 연결되지 않음"

#: plutils/cog.py:153 plutils/cog.py:185 plutils/cog.py:217 plutils/cog.py:248
msgid "Nothing playing"
msgstr "아무것도 재생하지 않음"

#: plutils/cog.py:168
#, docstring
msgid "Get the author of the current track"
msgstr "현재 트랙의 작성자 가져오기"

#: plutils/cog.py:200
#, docstring
msgid "Get the title of the current track"
msgstr "현재 트랙의 제목 가져오기"

#: plutils/cog.py:232
#, docstring
msgid "Get the source of the current track"
msgstr "현재 트랙의 소스 가져오기"

#: plutils/cog.py:264
#, docstring
msgid "Get the API of the current track"
msgstr "현재 트랙의 API 가져오기"

#: plutils/cog.py:284 plutils/cog.py:293
msgid "Unable to get player info"
msgstr "플레이어 정보를 가져올 수 없습니다."

#: plutils/cog.py:309
#, docstring
msgid "Decode a track's base64 string into a JSON object"
msgstr "트랙의 base64 문자열을 JSON 객체로 디코딩"

#: plutils/cog.py:318
msgid "Invalid base64 string"
msgstr "잘못된 base64 문자열"

#: plutils/cog.py:334
#, docstring
msgid "Manage the query cache"
msgstr "쿼리 캐시 관리"

#: plutils/cog.py:338
#, docstring
msgid "Clear the query cache"
msgstr "쿼리 캐시 지우기"

#: plutils/cog.py:345 plutils/cog.py:374 plutils/cog.py:387
msgid "Query cache cleared"
msgstr "쿼리 캐시가 지워짐"

#: plutils/cog.py:351
#, docstring
msgid "Clear the query cache older than a number of days"
msgstr "일 수보다 오래된 쿼리 캐시 지우기"

#: plutils/cog.py:359
msgid "Days must be less than 31"
msgstr "일 수는 31일 미만이어야 합니다."

#: plutils/cog.py:367
msgid "Days must be greater than 1"
msgstr "일 수는 1보다 커야 합니다."

#: plutils/cog.py:380
#, docstring
msgid "Clear the query cache for a query"
msgstr "쿼리에 대한 쿼리 캐시 지우기"

#: plutils/cog.py:393
#, docstring
msgid "Get the size of the query cache"
msgstr "쿼리 캐시의 크기 가져오기"

#: plutils/cog.py:400
msgid "Query cache size: `{size}`"
msgstr "쿼리 캐시 크기: '{size}`"

#: plutils/cog.py:411
#, docstring
msgid "Start memory tracing\n\n"
"        `[p]plutils trace 0` turns off tracing\n"
"        `[p]plutils trace 1` turns on tracing\n"
"        `[p]plutils trace` shows the current status of tracing\n"
"        "
msgstr "메모리 추적 시작\n\n"
"        `[p]Plutils 추적 0'은 추적을 끕니다.\n"
"        `[p]Plutils 추적 1'은 추적을 켭니다.\n"
"        `[p]Plutils 추적'은 추적의 현재 상태를 보여줍니다.\n"
"        "

#: plutils/cog.py:425
msgid "Stopped memory tracing"
msgstr "중지된 메모리 추적"

#: plutils/cog.py:434
msgid "Started memory tracing"
msgstr "메모리 추적 시작"

#: plutils/cog.py:443
msgid "You need to start tracing first"
msgstr "먼저 추적을 시작해야 합니다."

#: plutils/cog.py:455
#, docstring
msgid "Set the logger level\n\n"
"        Levels are the following:\n"
"        0: Critical\n"
"        1: Error\n"
"        2: Warning\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6: Trace\n"
"        "
msgstr "로거 수준 설정\n\n"
"        레벨은 다음과 같습니다.\n"
"        0: 위험\n"
"        1: 오류\n"
"        2: 경고\n"
"        3: 정보\n"
"        4: 디버그\n"
"        5: 자세한 정보 표시\n"
"        6: 추적\n"
"        "

#: plutils/cog.py:486
msgid "Logger level set to `{level}`"
msgstr "로거 수준이 '{level}`"

