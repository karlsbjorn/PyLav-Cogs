#: plutils/cog.py:72
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Toon de versie van de Cog en PyLav"

#: plutils/cog.py:88
msgid "Library / Cog"
msgstr "Bibliotheek / Cog"

#: plutils/cog.py:89
msgid "Version"
msgstr "Versie"

