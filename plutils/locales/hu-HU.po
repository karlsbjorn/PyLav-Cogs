#: plutils/cog.py:58 plutils/cog.py:68
#, docstring
msgid "Utility commands for PyLav"
msgstr "Segédprogram-parancsok a PyLav-hoz"

#: plutils/cog.py:72
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "A Cog és a PyLav verziójának megjelenítése"

#: plutils/cog.py:88
msgid "Library / Cog"
msgstr "Könyvtár / Fogaskerekű"

#: plutils/cog.py:89
msgid "Version"
msgstr "Verzió"

#: plutils/cog.py:102
#, docstring
msgid "Show the slashes available in the bot.\n\n"
"        Author: TrustyJAID#0001 via code block on Discord channel.\n"
"        "
msgstr "Mutassa meg a robotban elérhető perjeleket.\n\n"
"        Szerző: TrustyJAID #0001 kódblokkon keresztül a Discord csatornán.\n"
"        "

#: plutils/cog.py:133
#, docstring
msgid "Get info about specific things"
msgstr "Információ kérése konkrét dolgokról"

#: plutils/cog.py:137
#, docstring
msgid "Get the base64 of the current track"
msgstr "Az aktuális pálya base64-ének lekérése"

#: plutils/cog.py:145 plutils/cog.py:179 plutils/cog.py:213 plutils/cog.py:246
#: plutils/cog.py:281
msgid "You must be in a voice channel, so I can connect to it."
msgstr "Hangcsatornában kell lennie, hogy csatlakozhassak hozzá."

#: plutils/cog.py:154 plutils/cog.py:188 plutils/cog.py:222 plutils/cog.py:255
msgid "I am not currently playing anything on this server."
msgstr "Jelenleg nem játszom semmit ezen a szerveren."

#: plutils/cog.py:170
#, docstring
msgid "Get the author of the current track"
msgstr "Az aktuális szám szerzőjének lekérése"

#: plutils/cog.py:204
#, docstring
msgid "Get the title of the current track"
msgstr "Az aktuális szám címének lekérése"

#: plutils/cog.py:238
#, docstring
msgid "Get the source of the current track"
msgstr "Az aktuális szám forrásának lekérése"

#: plutils/cog.py:272
#, docstring
msgid "Get the API of the current track"
msgstr "Az aktuális szám API-jának lekérte"

#: plutils/cog.py:292 plutils/cog.py:301
msgid "Unable to get player info."
msgstr "Nem sikerült lekérni a játékos adatait."

#: plutils/cog.py:317
#, docstring
msgid "Decode the track base64 string into a JSON object"
msgstr "Dekódolja a track base64 sztringet egy JSON-objektumba"

#: plutils/cog.py:326
msgid "Invalid base64 string."
msgstr "Érvénytelen base64 karakterlánc."

#: plutils/cog.py:342
#, docstring
msgid "Manage the query cache"
msgstr "A lekérdezési gyorsítótár kezelése"

#: plutils/cog.py:346
#, docstring
msgid "Clear the query cache"
msgstr "A lekérdezési gyorsítótár törlése"

#: plutils/cog.py:353 plutils/cog.py:382 plutils/cog.py:395
msgid "Query cache cleared."
msgstr "A lekérdezési gyorsítótár törölve."

#: plutils/cog.py:359
#, docstring
msgid "Clear the query cache older than a number of days"
msgstr "Több napnál régebbi lekérdezési gyorsítótár törlése"

#: plutils/cog.py:367
msgid "Days must be less than 31."
msgstr "A napoknak 31-nél rövidebbnek kell lenniük."

#: plutils/cog.py:375
msgid "Days must be greater than 1."
msgstr "A napoknak 1-nél nagyobbnak kell lenniük."

#: plutils/cog.py:388
#, docstring
msgid "Clear the query cache for a query"
msgstr "Lekérdezés gyorsítótárának törlése"

#: plutils/cog.py:401
#, docstring
msgid "Get the size of the query cache"
msgstr "A lekérdezési gyorsítótár méretének lekérte"

#: plutils/cog.py:408
msgid "Query cache size: `{size_variable_do_not_translate}`."
msgstr "Lekérdezési gyorsítótár mérete: '{size_variable_do_not_translate}`."

#: plutils/cog.py:419
#, docstring
msgid "Start memory tracing\n\n"
"        `[p]plutils trace 0` turns off tracing\n"
"        `[p]plutils trace 1` turns on tracing\n"
"        `[p]plutils trace` shows the current status of tracing\n"
"        "
msgstr "Memória-nyomkövetés indítása\n\n"
"        `[p]A plutils trace 0' kikapcsolja a nyomkövetést\n"
"        `[p]plutils trace 1' bekapcsolja a nyomkövetést\n"
"        `[p]A Putils Trace' a nyomkövetés aktuális állapotát mutatja\n"
"        "

#: plutils/cog.py:433
msgid "I have stopped memory tracing"
msgstr "Leállítottam a memóriakövetést"

#: plutils/cog.py:442
msgid "I have started memory tracing."
msgstr "Elkezdtem a memória nyomon követését."

#: plutils/cog.py:451
msgid "You need to start tracing first."
msgstr "Először el kell kezdenie a nyomkövetést."

#: plutils/cog.py:463
#, docstring
msgid "Set the logger level\n\n"
"        Levels are the following:\n"
"        0: Critical\n"
"        1: Error\n"
"        2: Warning\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6: Trace\n"
"        "
msgstr "A naplózó szintjének beállítása\n\n"
"        A szintek a következők:\n"
"        0: Kritikus\n"
"        1: Hiba\n"
"        2: Figyelmeztetés\n"
"        3: Információ\n"
"        4: Hibakeresés\n"
"        5: Részletes\n"
"        6: Nyomkövetés\n"
"        "

#: plutils/cog.py:494
msgid "Logger level set to `{level_variable_do_not_translate}`."
msgstr "A naplózó szintje ' értékre van állítva{level_variable_do_not_translate}`."

