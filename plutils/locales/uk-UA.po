#: plutils/cog.py:58 plutils/cog.py:68
#, docstring
msgid "Utility commands for PyLav"
msgstr "Команди для PyLav"

#: plutils/cog.py:72
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Показати версію гвинтика і ПіЛава"

#: plutils/cog.py:88
msgid "Library / Cog"
msgstr "Бібліотека / Гвинтик"

#: plutils/cog.py:89
msgid "Version"
msgstr "Версія"

#: plutils/cog.py:102
#, docstring
msgid "Show the slashes available in the bot.\n\n"
"        Author: TrustyJAID#0001 via code block on Discord channel.\n"
"        "
msgstr "Показати слеші доступні в боті.\n\n"
"        Автор: TrustyJAID#0001 через код блоку в каналі Discord.\n"
"        "

#: plutils/cog.py:133
#, docstring
msgid "Get info about specific things"
msgstr "Отримати інформацію про конкретні речі"

#: plutils/cog.py:137
#, docstring
msgid "Get the base64 of the current track"
msgstr "Отримати base64 поточної композиції"

#: plutils/cog.py:145 plutils/cog.py:179 plutils/cog.py:213 plutils/cog.py:246
#: plutils/cog.py:281
msgid "You must be in a voice channel, so I can connect to it."
msgstr "Ви повинні бути в голосовому каналі, щоб я міг підключитися до нього."

#: plutils/cog.py:154 plutils/cog.py:188 plutils/cog.py:222 plutils/cog.py:255
msgid "I am not currently playing anything on this server."
msgstr "Зараз я нічого не граю на цьому сервері."

#: plutils/cog.py:170
#, docstring
msgid "Get the author of the current track"
msgstr "Отримати автора поточної пісні"

#: plutils/cog.py:204
#, docstring
msgid "Get the title of the current track"
msgstr "Отримати назву поточного треку"

#: plutils/cog.py:238
#, docstring
msgid "Get the source of the current track"
msgstr "Отримати джерело поточного треку"

#: plutils/cog.py:272
#, docstring
msgid "Get the API of the current track"
msgstr "Отримати API поточного треку"

#: plutils/cog.py:292 plutils/cog.py:301
msgid "Unable to get player info."
msgstr "Не вдається отримати інформацію про гравця."

#: plutils/cog.py:317
#, docstring
msgid "Decode the track base64 string into a JSON object"
msgstr "Розшифруйте рядок track base64 в об'єкт JSON"

#: plutils/cog.py:326
msgid "Invalid base64 string."
msgstr "Неприпустима основа64 рядок."

#: plutils/cog.py:342
#, docstring
msgid "Manage the query cache"
msgstr "Керування кешем запитів"

#: plutils/cog.py:346
#, docstring
msgid "Clear the query cache"
msgstr "Очистити кеш запитів"

#: plutils/cog.py:353 plutils/cog.py:382 plutils/cog.py:395
msgid "Query cache cleared."
msgstr "Кеш запиту очищено."

#: plutils/cog.py:359
#, docstring
msgid "Clear the query cache older than a number of days"
msgstr "Очистити кеш запитів старше ряду днів"

#: plutils/cog.py:367
msgid "Days must be less than 31."
msgstr "Днів має бути менше 31."

#: plutils/cog.py:375
msgid "Days must be greater than 1."
msgstr "Днів має бути більше 1."

#: plutils/cog.py:388
#, docstring
msgid "Clear the query cache for a query"
msgstr "Очистити кеш запиту"

#: plutils/cog.py:401
#, docstring
msgid "Get the size of the query cache"
msgstr "Отримати розмір кешу запитів"

#: plutils/cog.py:408
msgid "Query cache size: `{size_variable_do_not_translate}`."
msgstr "Розмір кешу запиту: '{size_variable_do_not_translate}`."

#: plutils/cog.py:419
#, docstring
msgid "Start memory tracing\n\n"
"        `[p]plutils trace 0` turns off tracing\n"
"        `[p]plutils trace 1` turns on tracing\n"
"        `[p]plutils trace` shows the current status of tracing\n"
"        "
msgstr "Запустити трасування оперативної пам'яті\n\n"
"        `[p]плутування 0` вимикає трасування\n"
"        `[p]стеження з лотками 1` повертає трасування\n"
"        `[p]плутія трасування показує поточний стан трасування\n"
"        "

#: plutils/cog.py:433
msgid "I have stopped memory tracing"
msgstr "Я припинив трасування пам'яті"

#: plutils/cog.py:442
msgid "I have started memory tracing."
msgstr "Я почав відстежувати пам'ять."

#: plutils/cog.py:451
msgid "You need to start tracing first."
msgstr "Починати розшук потрібно в першу чергу."

#: plutils/cog.py:463
#, docstring
msgid "Set the logger level\n\n"
"        Levels are the following:\n"
"        0: Critical\n"
"        1: Error\n"
"        2: Warning\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6: Trace\n"
"        "
msgstr "Встановити рівень журналювання\n\n"
"        Доступні такі рівні:\n"
"        0: Критичний\n"
"        1: Помилки\n"
"        2: Попередження\n"
"        3: Інформація\n"
"        4: Налагодження\n"
"        5: Багатослівний\n"
"        6: Відстежування\n"
"        "

#: plutils/cog.py:494
msgid "Logger level set to `{level_variable_do_not_translate}`."
msgstr "Рівень логгера встановлено на '{level_variable_do_not_translate}`."

