#: plutils/cog.py:58 plutils/cog.py:68
#, docstring
msgid "Utility commands for PyLav"
msgstr "Pomoćne naredbe za PyLav"

#: plutils/cog.py:72
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Prikaz verzije Cog i PyLav"

#: plutils/cog.py:88
msgid "Library / Cog"
msgstr "Knjižnica / Zupčanik"

#: plutils/cog.py:89
msgid "Version"
msgstr "Verzija"

#: plutils/cog.py:102
#, docstring
msgid "Show the slashes available in the bot.\n\n"
"        Author: TrustyJAID#0001 via code block on Discord channel.\n"
"        "
msgstr "Pokažite kose crte dostupne u botu.\n\n"
"        Autor: TrustyJAID#0001 putem bloka koda na Discord kanalu.\n"
"        "

#: plutils/cog.py:133
#, docstring
msgid "Get info about specific things"
msgstr "Informacije o određenim stvarima"

#: plutils/cog.py:137
#, docstring
msgid "Get the base64 of the current track"
msgstr "Dohvaćanje baze64 trenutnog zapisa"

#: plutils/cog.py:145 plutils/cog.py:179 plutils/cog.py:213 plutils/cog.py:246
#: plutils/cog.py:281
msgid "You must be in a voice channel, so I can connect to it."
msgstr "Morate biti u glasovnom kanalu, tako da se mogu povezati s njim."

#: plutils/cog.py:154 plutils/cog.py:188 plutils/cog.py:222 plutils/cog.py:255
msgid "I am not currently playing anything on this server."
msgstr "Trenutno ne igram ništa na ovom poslužitelju."

#: plutils/cog.py:170
#, docstring
msgid "Get the author of the current track"
msgstr "Dohvaćanje autora trenutne pjesme"

#: plutils/cog.py:204
#, docstring
msgid "Get the title of the current track"
msgstr "Dohvaćanje naslova trenutne pjesme"

#: plutils/cog.py:238
#, docstring
msgid "Get the source of the current track"
msgstr "Dohvaćanje izvora trenutne pjesme"

#: plutils/cog.py:272
#, docstring
msgid "Get the API of the current track"
msgstr "Dohvaćanje API-ja trenutne pjesme"

#: plutils/cog.py:292 plutils/cog.py:301
msgid "Unable to get player info."
msgstr "Nije moguće dohvatiti podatke o igraču."

#: plutils/cog.py:317
#, docstring
msgid "Decode the track base64 string into a JSON object"
msgstr "Dekodiranje niza baze zapisa64 u JSON objekt"

#: plutils/cog.py:326
msgid "Invalid base64 string."
msgstr "Niz base64 nije valjan."

#: plutils/cog.py:342
#, docstring
msgid "Manage the query cache"
msgstr "Upravljanje predmemorijom upita"

#: plutils/cog.py:346
#, docstring
msgid "Clear the query cache"
msgstr "Čišćenje predmemorije upita"

#: plutils/cog.py:353 plutils/cog.py:382 plutils/cog.py:395
msgid "Query cache cleared."
msgstr "Predmemorija upita je očišćena."

#: plutils/cog.py:359
#, docstring
msgid "Clear the query cache older than a number of days"
msgstr "Čišćenje predmemorije upita starije od nekoliko dana"

#: plutils/cog.py:367
msgid "Days must be less than 31."
msgstr "Dani moraju biti manji od 31."

#: plutils/cog.py:375
msgid "Days must be greater than 1."
msgstr "Dani moraju biti veći od 1."

#: plutils/cog.py:388
#, docstring
msgid "Clear the query cache for a query"
msgstr "Čišćenje predmemorije upita za upit"

#: plutils/cog.py:401
#, docstring
msgid "Get the size of the query cache"
msgstr "Dohvaćanje veličine predmemorije upita"

#: plutils/cog.py:408
msgid "Query cache size: `{size_variable_do_not_translate}`."
msgstr "Veličina predmemorije upita: '{size_variable_do_not_translate}`."

#: plutils/cog.py:419
#, docstring
msgid "Start memory tracing\n\n"
"        `[p]plutils trace 0` turns off tracing\n"
"        `[p]plutils trace 1` turns on tracing\n"
"        `[p]plutils trace` shows the current status of tracing\n"
"        "
msgstr "Pokreni praćenje memorije\n\n"
"        `[p]plutils trace 0' isključuje praćenje\n"
"        `[p]plutils trag 1' uključuje praćenje\n"
"        `[p]plutils trace' prikazuje trenutno stanje praćenja\n"
"        "

#: plutils/cog.py:433
msgid "I have stopped memory tracing"
msgstr "Zaustavio sam praćenje memorije"

#: plutils/cog.py:442
msgid "I have started memory tracing."
msgstr "Počeo sam s praćenjem memorije."

#: plutils/cog.py:451
msgid "You need to start tracing first."
msgstr "Prvo moraš početi pratiti."

#: plutils/cog.py:463
#, docstring
msgid "Set the logger level\n\n"
"        Levels are the following:\n"
"        0: Critical\n"
"        1: Error\n"
"        2: Warning\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6: Trace\n"
"        "
msgstr "Postavljanje razine zapisivača\n\n"
"        Razine su sljedeće:\n"
"        0: Kritično\n"
"        1: Pogreška\n"
"        2: Upozorenje\n"
"        3: Info\n"
"        4: Ispravljanje pogrešaka\n"
"        5: Verbose\n"
"        6: Praćenje\n"
"        "

#: plutils/cog.py:494
msgid "Logger level set to `{level_variable_do_not_translate}`."
msgstr "Razina zapisivača postavljena na '{level_variable_do_not_translate}`."

