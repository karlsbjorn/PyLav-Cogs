#: plutils/cog.py:58 plutils/cog.py:68
#, docstring
msgid "Utility commands for PyLav"
msgstr "คําสั่งยูทิลิตี้สําหรับ PyLav"

#: plutils/cog.py:72
#, docstring
msgid "Show the version of the Cog and its PyLav dependencies"
msgstr "แสดงเวอร์ชันของ Cog และการพึ่งพา PyLav"

#: plutils/cog.py:88
msgid "Library/Cog"
msgstr "ห้องสมุด/ฟันเฟือง"

#: plutils/cog.py:89
msgid "Version"
msgstr "เวอร์ชัน"

#: plutils/cog.py:102
#, docstring
msgid "Show the slashes available in the bot.\n\n"
"        Author: TrustyJAID#0001 via code block on Discord channel.\n"
"        "
msgstr "แสดงเครื่องหมายทับที่มีอยู่ในบอท\n\n"
"        ผู้เขียน: TrustyJAID#0001 ผ่านบล็อกรหัสในช่อง Discord\n"
"        "

#: plutils/cog.py:133
#, docstring
msgid "Get info about specific things"
msgstr "รับข้อมูลเกี่ยวกับสิ่งที่ต้องการ"

#: plutils/cog.py:137
#, docstring
msgid "Get the base64 of the current track"
msgstr "รับ base64 ของแทร็กปัจจุบัน"

#: plutils/cog.py:145 plutils/cog.py:177 plutils/cog.py:209 plutils/cog.py:240
#: plutils/cog.py:273
msgid "Not connected to a voice channel"
msgstr "ไม่ได้เชื่อมต่อกับช่องสัญญาณเสียง"

#: plutils/cog.py:153 plutils/cog.py:185 plutils/cog.py:217 plutils/cog.py:248
msgid "Nothing playing"
msgstr "ไม่มีอะไรเล่น"

#: plutils/cog.py:168
#, docstring
msgid "Get the author of the current track"
msgstr "รับผู้เขียนแทร็กปัจจุบัน"

#: plutils/cog.py:200
#, docstring
msgid "Get the title of the current track"
msgstr "รับชื่อของแทร็กปัจจุบัน"

#: plutils/cog.py:232
#, docstring
msgid "Get the source of the current track"
msgstr "ดูแหล่งที่มาของแทร็กปัจจุบัน"

#: plutils/cog.py:264
#, docstring
msgid "Get the API of the current track"
msgstr "รับ API ของแทร็กปัจจุบัน"

#: plutils/cog.py:284 plutils/cog.py:293
msgid "Unable to get player info"
msgstr "ไม่สามารถรับข้อมูลผู้เล่นได้"

#: plutils/cog.py:309
#, docstring
msgid "Decode a track's base64 string into a JSON object"
msgstr "ถอดรหัสสตริง base64 ของแทร็กลงในอ็อบเจ็กต์ JSON"

#: plutils/cog.py:318
msgid "Invalid base64 string"
msgstr "สตริง base64 ไม่ถูกต้อง"

#: plutils/cog.py:334
#, docstring
msgid "Manage the query cache"
msgstr "จัดการแคชคิวรี"

#: plutils/cog.py:338
#, docstring
msgid "Clear the query cache"
msgstr "ล้างแคชคิวรี"

#: plutils/cog.py:345 plutils/cog.py:374 plutils/cog.py:387
msgid "Query cache cleared"
msgstr "แคชคิวรีถูกล้างแล้ว"

#: plutils/cog.py:351
#, docstring
msgid "Clear the query cache older than a number of days"
msgstr "ล้างแคชคิวรีที่เก่ากว่าจํานวนวัน"

#: plutils/cog.py:359
msgid "Days must be less than 31"
msgstr "วันต้องน้อยกว่า 31"

#: plutils/cog.py:367
msgid "Days must be greater than 1"
msgstr "วันต้องมากกว่า 1"

#: plutils/cog.py:380
#, docstring
msgid "Clear the query cache for a query"
msgstr "ล้างแคชของคิวรีสําหรับคิวรี"

#: plutils/cog.py:393
#, docstring
msgid "Get the size of the query cache"
msgstr "ดูขนาดของแคชคิวรี"

#: plutils/cog.py:400
msgid "Query cache size: `{size}`"
msgstr "ขนาดแคชของคิวรี: '{size}`"

#: plutils/cog.py:411
#, docstring
msgid "Start memory tracing\n\n"
"        `[p]plutils trace 0` turns off tracing\n"
"        `[p]plutils trace 1` turns on tracing\n"
"        `[p]plutils trace` shows the current status of tracing\n"
"        "
msgstr "เริ่มการติดตามหน่วยความจํา\n\n"
"        `[p]plutils ติดตาม 0'ปิดการติดตาม\n"
"        `[p]plutils ติดตาม 1'เปิดในการติดตาม\n"
"        `[p]การติดตาม plutils' แสดงสถานะปัจจุบันของการติดตาม\n"
"        "

#: plutils/cog.py:425
msgid "Stopped memory tracing"
msgstr "หยุดการติดตามหน่วยความจํา"

#: plutils/cog.py:434
msgid "Started memory tracing"
msgstr "เริ่มการติดตามหน่วยความจํา"

#: plutils/cog.py:443
msgid "You need to start tracing first"
msgstr "คุณต้องเริ่มติดตามก่อน"

#: plutils/cog.py:455
#, docstring
msgid "Set the logger level\n\n"
"        Levels are the following:\n"
"        0: Critical\n"
"        1: Error\n"
"        2: Warning\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6: Trace\n"
"        "
msgstr "ตั้งค่าระดับคนตัดไม้\n\n"
"        ระดับมีดังต่อไปนี้:\n"
"        0: สําคัญ\n"
"        1: ข้อผิดพลาด\n"
"        2: คําเตือน\n"
"        3: ข้อมูล\n"
"        4: ดีบัก\n"
"        5: คําฟุ่มเฟือย\n"
"        6: ติดตาม\n"
"        "

#: plutils/cog.py:486
msgid "Logger level set to `{level}`"
msgstr "ตั้งค่าระดับคนตัดไม้เป็น '{level}`"

