#: plutils/cog.py:58 plutils/cog.py:68
#, docstring
msgid "Utility commands for PyLav"
msgstr "Comandi di utilità per PyLav"

#: plutils/cog.py:72
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Mostra la versione di Cog e PyLav"

#: plutils/cog.py:88
msgid "Library / Cog"
msgstr "Biblioteca / Ingranaggio"

#: plutils/cog.py:89
msgid "Version"
msgstr "Versione"

#: plutils/cog.py:102
#, docstring
msgid "Show the slashes available in the bot.\n\n"
"        Author: TrustyJAID#0001 via code block on Discord channel.\n"
"        "
msgstr "Mostra le barre disponibili nel bot.\n\n"
"        Autore: TrustyJAID#0001 tramite blocco di codice sul canale Discord.\n"
"        "

#: plutils/cog.py:133
#, docstring
msgid "Get info about specific things"
msgstr "Ottieni informazioni su cose specifiche"

#: plutils/cog.py:137
#, docstring
msgid "Get the base64 of the current track"
msgstr "Ottieni la base64 della traccia corrente"

#: plutils/cog.py:145 plutils/cog.py:179 plutils/cog.py:213 plutils/cog.py:246
#: plutils/cog.py:281
msgid "You must be in a voice channel, so I can connect to it."
msgstr "Devi essere in un canale vocale, in modo che io possa connettermi ad esso."

#: plutils/cog.py:154 plutils/cog.py:188 plutils/cog.py:222 plutils/cog.py:255
msgid "I am not currently playing anything on this server."
msgstr "Al momento non sto giocando nulla su questo server."

#: plutils/cog.py:170
#, docstring
msgid "Get the author of the current track"
msgstr "Ottiene l'autore della traccia corrente"

#: plutils/cog.py:204
#, docstring
msgid "Get the title of the current track"
msgstr "Ottiene il titolo della traccia corrente"

#: plutils/cog.py:238
#, docstring
msgid "Get the source of the current track"
msgstr "Ottieni la sorgente della traccia corrente"

#: plutils/cog.py:272
#, docstring
msgid "Get the API of the current track"
msgstr "Ottieni l'API della traccia corrente"

#: plutils/cog.py:292 plutils/cog.py:301
msgid "Unable to get player info."
msgstr "Impossibile ottenere informazioni sul giocatore."

#: plutils/cog.py:317
#, docstring
msgid "Decode the track base64 string into a JSON object"
msgstr "Decodificare la stringa track base64 in un oggetto JSON"

#: plutils/cog.py:326
msgid "Invalid base64 string."
msgstr "Stringa base64 non valida."

#: plutils/cog.py:342
#, docstring
msgid "Manage the query cache"
msgstr "Gestisci la cache delle query"

#: plutils/cog.py:346
#, docstring
msgid "Clear the query cache"
msgstr "Pulisci la cache della query"

#: plutils/cog.py:353 plutils/cog.py:382 plutils/cog.py:395
msgid "Query cache cleared."
msgstr "Cache delle query cancellata."

#: plutils/cog.py:359
#, docstring
msgid "Clear the query cache older than a number of days"
msgstr "Cancella la cache della query più vecchia di un numero di giorni"

#: plutils/cog.py:367
msgid "Days must be less than 31."
msgstr "I giorni devono essere inferiori a 31."

#: plutils/cog.py:375
msgid "Days must be greater than 1."
msgstr "I giorni devono essere maggiori di 1."

#: plutils/cog.py:388
#, docstring
msgid "Clear the query cache for a query"
msgstr "Pulisce la cache della query per una query"

#: plutils/cog.py:401
#, docstring
msgid "Get the size of the query cache"
msgstr "Ottieni la dimensione della cache di query"

#: plutils/cog.py:408
msgid "Query cache size: `{size_variable_do_not_translate}`."
msgstr "Query cache size: '{size_variable_do_not_translate}`."

#: plutils/cog.py:419
#, docstring
msgid "Start memory tracing\n\n"
"        `[p]plutils trace 0` turns off tracing\n"
"        `[p]plutils trace 1` turns on tracing\n"
"        `[p]plutils trace` shows the current status of tracing\n"
"        "
msgstr "Avvia il tracciamento della memoria\n\n"
"        `[p]plutils trace 0` disattiva il tracciamento\n"
"        `[p]plutils trace 1` attiva il tracciamento\n"
"        `[p]plutils trace` mostra lo stato corrente del tracciamento\n"
"        "

#: plutils/cog.py:433
msgid "I have stopped memory tracing"
msgstr "Ho interrotto il tracciamento della memoria"

#: plutils/cog.py:442
msgid "I have started memory tracing."
msgstr "Ho iniziato a tracciare la memoria."

#: plutils/cog.py:451
msgid "You need to start tracing first."
msgstr "È necessario iniziare prima a tracciare."

#: plutils/cog.py:463
#, docstring
msgid "Set the logger level\n\n"
"        Levels are the following:\n"
"        0: Critical\n"
"        1: Error\n"
"        2: Warning\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6: Trace\n"
"        "
msgstr "Impostare il livello di logger\n\n"
"        I livelli sono i seguenti:\n"
"        0: Critico\n"
"        1: Errore\n"
"        2: Attenzione\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6: Traccia\n"
"        "

#: plutils/cog.py:494
msgid "Logger level set to `{level_variable_do_not_translate}`."
msgstr "Livello logger impostato su '{level_variable_do_not_translate}`."

