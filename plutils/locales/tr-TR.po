#: plutils/cog.py:334
#, docstring
msgid "Manage the query cache"
msgstr "Sorgu önbelleğini yönetme"

#: plutils/cog.py:338
#, docstring
msgid "Clear the query cache"
msgstr "Sorgu önbelleğini temizleme"

#: plutils/cog.py:345 plutils/cog.py:374 plutils/cog.py:387
msgid "Query cache cleared"
msgstr "Sorgu önbelleği temizlendi"

#: plutils/cog.py:351
#, docstring
msgid "Clear the query cache older than a number of days"
msgstr "Birkaç günden daha eski sorgu önbelleğini temizleme"

#: plutils/cog.py:359
msgid "Days must be less than 31"
msgstr "Gün 31'den az olmalıdır"

#: plutils/cog.py:367
msgid "Days must be greater than 1"
msgstr "Gün 1'den büyük olmalıdır"

#: plutils/cog.py:380
#, docstring
msgid "Clear the query cache for a query"
msgstr "Sorgu için sorgu önbelleğini temizleme"

#: plutils/cog.py:393
#, docstring
msgid "Get the size of the query cache"
msgstr "Sorgu önbelleğinin boyutunu alma"

#: plutils/cog.py:400
msgid "Query cache size: `{size}`"
msgstr "Sorgu önbelleği boyutu: '{size}`"

#: plutils/cog.py:411
#, docstring
msgid "Start memory tracing\n\n"
"        `[p]plutils trace 0` turns off tracing\n"
"        `[p]plutils trace 1` turns on tracing\n"
"        `[p]plutils trace` shows the current status of tracing\n"
"        "
msgstr "Bellek izlemeyi başlatma\n\n"
"        `[p]plutils trace 0' izlemeyi kapatır\n"
"        `[p]plutils trace 1' izlemeyi açar\n"
"        `[p]plutils trace' izlemenin mevcut durumunu gösterir\n"
"        "

#: plutils/cog.py:425
msgid "Stopped memory tracing"
msgstr "Bellek izleme durduruldu"

#: plutils/cog.py:434
msgid "Started memory tracing"
msgstr "Bellek izleme başlatıldı"

#: plutils/cog.py:443
msgid "You need to start tracing first"
msgstr "Önce izlemeye başlamanız gerekir"

#: plutils/cog.py:455
#, docstring
msgid "Set the logger level\n\n"
"        Levels are the following:\n"
"        0: Critical\n"
"        1: Error\n"
"        2: Warning\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6: Trace\n"
"        "
msgstr "Logger düzeyini ayarlama\n\n"
"        Seviyeler şunlardır:\n"
"        0: Kritik\n"
"        1: Hata\n"
"        2: Uyarı\n"
"        3: Bilgi\n"
"        4: Hata Ayıklama\n"
"        5: Ayrıntılı\n"
"        6: İzleme\n"
"        "

#: plutils/cog.py:486
msgid "Logger level set to `{level}`"
msgstr "Logger seviyesi ' olarak ayarlandı{level}`"

