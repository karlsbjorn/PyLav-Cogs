#: plutils/cog.py:58 plutils/cog.py:68
#, docstring
msgid "Utility commands for PyLav"
msgstr "Εντολές χρησιμότητας για το PyLav"

#: plutils/cog.py:72
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Δείξτε την έκδοση του Γραναζιού και του PyLav"

#: plutils/cog.py:88
msgid "Library / Cog"
msgstr "Βιβλιοθήκη / Γρανάζι"

#: plutils/cog.py:89
msgid "Version"
msgstr "Εκδοχή"

#: plutils/cog.py:102
#, docstring
msgid "Show the slashes available in the bot.\n\n"
"        Author: TrustyJAID#0001 via code block on Discord channel.\n"
"        "
msgstr "Εμφάνιση των κάθετων διαθέσιμων στο bot.\n\n"
"        Συγγραφέας: TrustyJAID#0001 μέσω του μπλοκ κώδικα στο κανάλι Discord.\n"
"        "

#: plutils/cog.py:133
#, docstring
msgid "Get info about specific things"
msgstr "Λάβετε πληροφορίες σχετικά με συγκεκριμένα πράγματα"

#: plutils/cog.py:137
#, docstring
msgid "Get the base64 of the current track"
msgstr "Αποκτήστε το base64 του τρέχοντος κομματιού"

#: plutils/cog.py:145 plutils/cog.py:179 plutils/cog.py:213 plutils/cog.py:246
#: plutils/cog.py:281
msgid "You must be in a voice channel, so I can connect to it."
msgstr "Πρέπει να βρίσκεστε σε ένα κανάλι φωνής, ώστε να μπορώ να συνδεθώ σε αυτό."

#: plutils/cog.py:154 plutils/cog.py:188 plutils/cog.py:222 plutils/cog.py:255
msgid "I am not currently playing anything on this server."
msgstr "Αυτή τη στιγμή δεν παίζω τίποτα σε αυτόν τον διακομιστή."

#: plutils/cog.py:170
#, docstring
msgid "Get the author of the current track"
msgstr "Λήψη του συγγραφέα του τρέχοντος κομματιού"

#: plutils/cog.py:204
#, docstring
msgid "Get the title of the current track"
msgstr "Λήψη τίτλου του τρέχοντος κομματιού"

#: plutils/cog.py:238
#, docstring
msgid "Get the source of the current track"
msgstr "Λήψη πηγής του τρέχοντος κομματιού"

#: plutils/cog.py:272
#, docstring
msgid "Get the API of the current track"
msgstr "Λήψη του API του τρέχοντος κομματιού"

#: plutils/cog.py:292 plutils/cog.py:301
msgid "Unable to get player info."
msgstr "Δεν είναι δυνατή η λήψη πληροφοριών παίκτη."

#: plutils/cog.py:317
#, docstring
msgid "Decode the track base64 string into a JSON object"
msgstr "Αποκωδικοποίηση της συμβολοσειράς base64 κομματιού σε αντικείμενο JSON"

#: plutils/cog.py:326
msgid "Invalid base64 string."
msgstr "Μη έγκυρη συμβολοσειρά base64."

#: plutils/cog.py:342
#, docstring
msgid "Manage the query cache"
msgstr "Διαχείριση της λανθάνουσας μνήμης ερωτήματος"

#: plutils/cog.py:346
#, docstring
msgid "Clear the query cache"
msgstr "Καθαρισμός της λανθάνουσας μνήμης ερωτήματος"

#: plutils/cog.py:353 plutils/cog.py:382 plutils/cog.py:395
msgid "Query cache cleared."
msgstr "Εκκαθαρίστηκε το cache ερωτημάτων."

#: plutils/cog.py:359
#, docstring
msgid "Clear the query cache older than a number of days"
msgstr "Εκκαθάριση της προσωρινής μνήμης ερωτημάτων παλαιότερης από έναν αριθμό ημερών"

#: plutils/cog.py:367
msgid "Days must be less than 31."
msgstr "Οι ημέρες πρέπει να είναι μικρότερες από 31."

#: plutils/cog.py:375
msgid "Days must be greater than 1."
msgstr "Οι ημέρες πρέπει να είναι μεγαλύτερες από 1."

#: plutils/cog.py:388
#, docstring
msgid "Clear the query cache for a query"
msgstr "Εκκαθάριση προσωρινής μνήμης ερωτήματος για ένα ερώτημα"

#: plutils/cog.py:401
#, docstring
msgid "Get the size of the query cache"
msgstr "Λήψη του μεγέθους της λανθάνουσας μνήμης ερωτημάτων"

#: plutils/cog.py:408
msgid "Query cache size: `{size_variable_do_not_translate}`."
msgstr "Μέγεθος cache ερωτήματος: '{size_variable_do_not_translate}`."

#: plutils/cog.py:419
#, docstring
msgid "Start memory tracing\n\n"
"        `[p]plutils trace 0` turns off tracing\n"
"        `[p]plutils trace 1` turns on tracing\n"
"        `[p]plutils trace` shows the current status of tracing\n"
"        "
msgstr "Έναρξη ανίχνευσης μνήμης\n\n"
"        `[p]plutils trace 0` απενεργοποιεί την ανίχνευση\n"
"        `[p]plutils trace 1` ενεργοποιεί την ανίχνευση\n"
"        `[p]ίχνη plutil` δείχνει την τρέχουσα κατάσταση της ανίχνευσης\n"
"        "

#: plutils/cog.py:433
msgid "I have stopped memory tracing"
msgstr "Σταμάτησα την ανίχνευση μνήμης"

#: plutils/cog.py:442
msgid "I have started memory tracing."
msgstr "Έχω ξεκινήσει την ανίχνευση μνήμης."

#: plutils/cog.py:451
msgid "You need to start tracing first."
msgstr "Πρέπει πρώτα να ξεκινήσετε την ιχνηλάτηση."

#: plutils/cog.py:463
#, docstring
msgid "Set the logger level\n\n"
"        Levels are the following:\n"
"        0: Critical\n"
"        1: Error\n"
"        2: Warning\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6: Trace\n"
"        "
msgstr "Ορίστε το επίπεδο καταγραφέα\n\n"
"        Επίπεδα είναι τα ακόλουθα:\n"
"        0: Κρίσιμο\n"
"        1: Σφάλμα\n"
"        2: Προειδοποίηση\n"
"        3: Πληροφορίες\n"
"        4: Αποσφαλμάτωση\n"
"        5: Ρήμα\n"
"        6: Ίχνος\n"
"        "

#: plutils/cog.py:494
msgid "Logger level set to `{level_variable_do_not_translate}`."
msgstr "Επίπεδο καταγραφικού ρυθμισμένο σε '{level_variable_do_not_translate}`."

