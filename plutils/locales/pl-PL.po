#: plutils/cog.py:58 plutils/cog.py:68
#, docstring
msgid "Utility commands for PyLav"
msgstr "Polecenia użyteczności dla PyLav"

#: plutils/cog.py:72
#, docstring
msgid "Show the version of the Cog and PyLav"
msgstr "Pokaż wersję Cog i PyLav"

#: plutils/cog.py:88
msgid "Library / Cog"
msgstr "Biblioteka / Cog"

#: plutils/cog.py:89
msgid "Version"
msgstr "Wersja"

#: plutils/cog.py:102
#, docstring
msgid "Show the slashes available in the bot.\n\n"
"        Author: TrustyJAID#0001 via code block on Discord channel.\n"
"        "
msgstr "Pokaż ukośniki dostępne w botze.\n\n"
"        Autor: TrustyJAID#0001 poprzez blok kodu na kanale Discord.\n"
"        "

#: plutils/cog.py:133
#, docstring
msgid "Get info about specific things"
msgstr "Uzyskaj informacje o konkretnych rzeczach"

#: plutils/cog.py:137
#, docstring
msgid "Get the base64 of the current track"
msgstr "Zdobądź base64 bieżącego utworu"

#: plutils/cog.py:145 plutils/cog.py:179 plutils/cog.py:213 plutils/cog.py:246
#: plutils/cog.py:281
msgid "You must be in a voice channel, so I can connect to it."
msgstr "Musisz być w kanale głosowym, abym mógł się z nim połączyć."

#: plutils/cog.py:154 plutils/cog.py:188 plutils/cog.py:222 plutils/cog.py:255
msgid "I am not currently playing anything on this server."
msgstr "Obecnie nie gram w nic na tym serwerze."

#: plutils/cog.py:170
#, docstring
msgid "Get the author of the current track"
msgstr "Pobierz autora bieżącego utworu"

#: plutils/cog.py:204
#, docstring
msgid "Get the title of the current track"
msgstr "Pobierz tytuł bieżącego utworu"

#: plutils/cog.py:238
#, docstring
msgid "Get the source of the current track"
msgstr "Pobierz źródło bieżącego utworu"

#: plutils/cog.py:272
#, docstring
msgid "Get the API of the current track"
msgstr "Pobierz API bieżącego utworu"

#: plutils/cog.py:292 plutils/cog.py:301
msgid "Unable to get player info."
msgstr "Nie można uzyskać informacji o graczu."

#: plutils/cog.py:317
#, docstring
msgid "Decode the track base64 string into a JSON object"
msgstr "Dekoduj ciąg track base64 na obiekt JSON"

#: plutils/cog.py:326
msgid "Invalid base64 string."
msgstr "Nieprawidłowy ciąg base64."

#: plutils/cog.py:342
#, docstring
msgid "Manage the query cache"
msgstr "Zarządzaj skrzynką zapytania"

#: plutils/cog.py:346
#, docstring
msgid "Clear the query cache"
msgstr "Wyczyść cache zapytania"

#: plutils/cog.py:353 plutils/cog.py:382 plutils/cog.py:395
msgid "Query cache cleared."
msgstr "Wyczyszczono pamięć podręczną zapytań."

#: plutils/cog.py:359
#, docstring
msgid "Clear the query cache older than a number of days"
msgstr "Wyczyść pamięć podręczną zapytania starszą niż liczba dni"

#: plutils/cog.py:367
msgid "Days must be less than 31."
msgstr "Liczba dni musi być krótsza niż 31."

#: plutils/cog.py:375
msgid "Days must be greater than 1."
msgstr "Liczba dni musi być większa niż 1."

#: plutils/cog.py:388
#, docstring
msgid "Clear the query cache for a query"
msgstr "Wyczyść pamięć podręczną dla zapytania"

#: plutils/cog.py:401
#, docstring
msgid "Get the size of the query cache"
msgstr "Pobierz rozmiar pamięci podręcznej zapytania"

#: plutils/cog.py:408
msgid "Query cache size: `{size_variable_do_not_translate}`."
msgstr "Rozmiar pamięci podręcznej zapytania: '{size_variable_do_not_translate}`."

#: plutils/cog.py:419
#, docstring
msgid "Start memory tracing\n\n"
"        `[p]plutils trace 0` turns off tracing\n"
"        `[p]plutils trace 1` turns on tracing\n"
"        `[p]plutils trace` shows the current status of tracing\n"
"        "
msgstr "Rozpocznij śledzenie pamięci\n\n"
"        `[p]plutils trace 0` wyłącza śledzenie\n"
"        `[p]plutils trace 1` włącza śledzenie\n"
"        `[p]plutils trace` pokazuje aktualny status śledzenia\n"
"        "

#: plutils/cog.py:433
msgid "I have stopped memory tracing"
msgstr "Zatrzymałem śledzenie pamięci"

#: plutils/cog.py:442
msgid "I have started memory tracing."
msgstr "Rozpocząłem śledzenie pamięci."

#: plutils/cog.py:451
msgid "You need to start tracing first."
msgstr "Najpierw musisz rozpocząć śledzenie."

#: plutils/cog.py:463
#, docstring
msgid "Set the logger level\n\n"
"        Levels are the following:\n"
"        0: Critical\n"
"        1: Error\n"
"        2: Warning\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6: Trace\n"
"        "
msgstr "Ustaw poziom rejestratora\n\n"
"        Poziomy są następujące:\n"
"        0: Krytyczny\n"
"        1: Błąd\n"
"        2: Ostrzeżenie\n"
"        3: Info\n"
"        4: Debugowanie\n"
"        5: Verbose\n"
"        6: Śledź\n"
"        "

#: plutils/cog.py:494
msgid "Logger level set to `{level_variable_do_not_translate}`."
msgstr "Poziom rejestratora ustawiony na '{level_variable_do_not_translate}`."

