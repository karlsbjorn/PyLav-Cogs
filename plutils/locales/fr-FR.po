#: plutils/cog.py:342
#, docstring
msgid "Manage the query cache"
msgstr "Gérer le cache des requêtes"

#: plutils/cog.py:346
#, docstring
msgid "Clear the query cache"
msgstr "Vider le cache des requêtes"

#: plutils/cog.py:353 plutils/cog.py:382 plutils/cog.py:395
msgid "Query cache cleared."
msgstr "Cache de requête effacé."

#: plutils/cog.py:359
#, docstring
msgid "Clear the query cache older than a number of days"
msgstr "Vider le cache de requête plus ancien que plusieurs jours"

#: plutils/cog.py:367
msgid "Days must be less than 31."
msgstr "Les jours doivent être inférieurs à 31."

#: plutils/cog.py:375
msgid "Days must be greater than 1."
msgstr "Les jours doivent être supérieurs à 1."

#: plutils/cog.py:388
#, docstring
msgid "Clear the query cache for a query"
msgstr "Vider le cache de requête pour une requête"

#: plutils/cog.py:401
#, docstring
msgid "Get the size of the query cache"
msgstr "Récupère la taille du cache des requêtes"

#: plutils/cog.py:408
msgid "Query cache size: `{size_variable_do_not_translate}`."
msgstr "Taille du cache de requête : '{size_variable_do_not_translate}`."

#: plutils/cog.py:419
#, docstring
msgid "Start memory tracing\n\n"
"        `[p]plutils trace 0` turns off tracing\n"
"        `[p]plutils trace 1` turns on tracing\n"
"        `[p]plutils trace` shows the current status of tracing\n"
"        "
msgstr "Démarrer le traçage de la mémoire\n\n"
"        `[p]plutils trace 0` désactive le traçage\n"
"        `[p]plutils trace 1` active le traçage\n"
"        `[p]plutils trace` affiche l'état actuel du traçage\n"
"        "

#: plutils/cog.py:433
msgid "I have stopped memory tracing"
msgstr "J’ai arrêté le traçage de mémoire"

#: plutils/cog.py:442
msgid "I have started memory tracing."
msgstr "J’ai commencé le traçage de la mémoire."

#: plutils/cog.py:451
msgid "You need to start tracing first."
msgstr "Vous devez d’abord commencer le traçage."

#: plutils/cog.py:463
#, docstring
msgid "Set the logger level\n\n"
"        Levels are the following:\n"
"        0: Critical\n"
"        1: Error\n"
"        2: Warning\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6: Trace\n"
"        "
msgstr "Définissez le niveau de enregistreur\n\n"
"        Les niveaux sont les suivants :\n"
"        0 : Critique\n"
"        1: Erreur\n"
"        2: Avertissement\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6 : Trace\n"
"        "

#: plutils/cog.py:494
msgid "Logger level set to `{level_variable_do_not_translate}`."
msgstr "Niveau de l’enregistreur défini sur '{level_variable_do_not_translate}`."

